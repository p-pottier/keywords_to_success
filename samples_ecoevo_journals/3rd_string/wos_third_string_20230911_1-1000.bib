@article{ WOS:000808220700002,
Author = {Collins, Timothy L. and Schmidt-Lebuhn, Alexander N. and Andrew, Rose L.
   and Telford, Ian R. H. and Bruhl, Jeremy J.},
Title = {There's gold in them thar hills! Morphology and molecules delimit
   species in Xerochrysum (Asteraceae; Gnaphalieae) and reveal many new
   taxa},
Journal = {AUSTRALIAN SYSTEMATIC BOTANY},
Year = {2022},
Volume = {35},
Number = {2},
Pages = {120-185},
Abstract = {Golden everlasting paper daisies in the genus Xerochrysum Tzvelev are
   iconic Australian native plants grown worldwide. The X. bracteatum
   species complex has been regarded as taxonomically confusing and in need
   of revision for over 60 years. We applied morphological and molecular
   analyses to delimit species, detect common ancestry among populations,
   and identify putative hybrids in the genus Xerochrysum (Asteraceae:
   Gnaphalieae). Multiple lines of evidence provided strong support for the
   recognition of new taxa. Here we describe the following 11 new species:
   X. andrewiae T.L.Collins \& J.J.Bruhl, X. berarngutta T.L.Collins \&
   I.Telford, X. copelandii J.J.Bruhl \& I.Telford, X. frutescens J.J.Bruhl
   \& I.Telford, X. gudang T.L.Collins \& J.J.Bruhl, X. hispidum
   T.L.Collins \& I.Telford, X. macsweeneyorum T.L.Collins, X. murapan
   T.L.Collins \& I.Telford, X. neoanglicum J.J.Bruhl \& I.Telford, X.
   strictum T.L.Collins, and X. wilsonii T.L.Collins, reinstate Helichrysum
   banksii A.Cunn. ex DC. (as X. banksii (A.Cunn. ex DC.) T.L.Collins \&
   I.Telford), lectotypify X. banksii and X. papillosum (Labill.)
   R.J.Bayer, and recircumscribe X. bicolor (Lindl.) R.J.Bayer to include
   X. halmaturorum Paul G.Wilson and some populations of X. bracteatum
   sens. lat. from mainland South Australia and Victoria. We also provide
   revised descriptions of all taxa in the genus, their conservation
   status, a dichotomous key, tables distinguishing closely related taxa
   and distribution maps.},
