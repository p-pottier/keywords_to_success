@article{ WOS:000859833100001,
Author = {Kelly, Clint D.},
Title = {Can Patterns of Static Allometry be Inferred from Regimes of Sexual
   Selection in the Japanese Beetle?},
Journal = {EVOLUTIONARY BIOLOGY},
Year = {2022},
Volume = {49},
Number = {4},
Pages = {403-413},
Month = {DEC},
Abstract = {Evolutionary biologists have long posited that secondary sexual
   characters experiencing positive directional selection should be
   positively allometric. Support for this hypothesis, however, is limited
   and appears to hold under a specific set of conditions. An alternative
   hypothesis argues that different forms of sexual selection can produce
   similar patterns of allometry, and vice versa, depending on the net
   effect of sexual selection and viability selection. We currently lack a
   general understanding of how particular patterns of allometry emerge
   from particular sexual selection regimes. Here, I empirically examine
   whether patterns of static allometry exhibited by leg and wing traits in
   female and male Japanese beetles (Popillia japonica) align with the form
   of sexual selection underlying them. Contrary to expectation, I found
   that patterns of allometry did not universally align with their regime
   of sexual selection. Despite the direction and strength of sexual
   selection differing between the sexes, I found that all traits were
   negatively allometric with the exception of tibia width in males, which
   was isometric. Patterns of allometry for tibia length and wing traits
   were sexually dimorphic under some conditions and the slopes for wing
   traits were steeper in mated than in unmated males. My results support
   the hypothesis that different sexual selection regimes can produce
   similar patterns of static allometry and suggest that viability
   selection on these traits might be stronger than sexual selection and
   could thus be responsible for shaping their patterns of static
   allometry.},
