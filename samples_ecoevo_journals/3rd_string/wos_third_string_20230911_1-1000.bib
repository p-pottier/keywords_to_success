@article{ WOS:000828399600005,
Author = {Hale, Clayton W. and Granger, Joshua J. and Siegert, Courtney M. and
   Correa, Sandra B. and DuBien, Janice L. and Goldsmith, Caleb D. and
   Kruckeberg, Will},
Title = {Microsite Habitat, Species Associations, and Habitat Suitability Model
   of a Globally Imperiled Shrub},
