@article{ WOS:000806844100005,
Author = {Nienartowicz, Andrzej and Rutkowski, Lucjan and Kaminski, Dariusz and
   Kunz, Mieczyslaw},
Title = {Occurrence of Viscum album subsp. album L. on Laburnum anagyroides
   Medik. in Torun, Poland},
Journal = {ECOLOGICAL QUESTIONS},
Year = {2022},
Volume = {33},
Number = {1},
Abstract = {This short note presents two localities of the common mistletoe growing
   on the common laburnum found in central Poland in the city of Torun in
   2020. The common laburnum is one of the rare hosts of this recently
