@article{ WOS:000748941600001,
Author = {Gardner, Nicholas M. and Dececchi, T. Alexander},
Title = {Flight and echolocation evolved once in Chiroptera: comments on `The
   evolution of flight in bats: a novel hypothesis' COMMENT},
Journal = {MAMMAL REVIEW},
Year = {2022},
Volume = {52},
Number = {2},
Pages = {284-290},
Month = {APR},
Abstract = {Anderson and Ruxton (Mammal Review, 2020) reviewed the evolution of
   powered flight and laryngeal echolocation in bats. They hypothesised
   that powered flight and laryngeal echolocation evolved in separate
   lineages of handwing gliders. We note that fossil, character evolution,
   and developmental evidence contradicts their hypothesis, and we test
   their handwing gliding model, finding it aerodynamically implausible. We
   conclude that the traditional view of bat evolution (that flight and
   laryngeal echolocation evolved in the common ancestor of all bats, with
   the latter being lost in pteropodids) is more plausible than the
   proposed novel hypothesis.},
