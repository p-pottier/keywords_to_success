@article{ WOS:000868347300004,
Author = {Pena-Pena, Karina and Irmler, Ulrich},
Title = {Modelling litter decomposition rates along a geographic, climatic, and
   land-use gradient in Southern Amazonia and Para acute accent},
Journal = {PEDOBIOLOGIA},
Year = {2022},
Volume = {95},
Month = {DEC},
Abstract = {The study aimed to investigate the effect on litter decomposition of the
   geographical and land-use gradient from the dry Cerrado in Mato Grosso
   to the southern Amazonian rainforest in Par ` a, Brazil. The study area
   also rep-resents a gradient of land-use intensification, reaching from
   the southern Cerrado region which had the most active land use change
   and agricultural intensification in the past to the northern rainforest
   region where agricultural use is currently intensifying. We assessed the
   effects on litter decomposition of climate (seasonality: rainy vs dry),
   litter quality (litter type: standard maize litter vs local habitat
   litter), disturbance (land-use: forest vs pasture vs cropland), soil
   organisms (soil fauna: inclusion vs exclusion), and original land
