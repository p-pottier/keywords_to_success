@article{ WOS:000816973900001,
Author = {He, Mengxin and Li, Zhong},
Title = {Stability of a fear effect predator-prey model with mutual interference
   or group defense},
Journal = {JOURNAL OF BIOLOGICAL DYNAMICS},
Year = {2022},
Volume = {16},
Number = {1},
Pages = {480-498},
Month = {DEC 31},
Abstract = {In this paper, we consider a fear effect predator-prey model with mutual
   interference or group defense. For the model with mutual interference,
   we show the interior equilibrium is globally stable, and the mutual
   interference can stabilize the predator-prey system. For the model with
   group defense, we discuss the singular dynamics around the origin and
   the occurrence of Hopf bifurcation, and find that there is a separatrix
   curve near the origin such that the orbits above which tend to the
   origin and the orbits below which tend to limit cycle or the interior
   equilibrium.},
