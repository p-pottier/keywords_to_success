@article{ WOS:000773530000001,
Author = {Liu, Peijiang and Huang, Lifang and Din, Anwarud and Huang, Xiangxiang},
Title = {Impact of information and Levy noise on stochastic COVID-19 epidemic
   model under real statistical data},
