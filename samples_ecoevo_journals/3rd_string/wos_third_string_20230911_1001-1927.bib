@article{ WOS:000837522400001,
Author = {Tanaka, Kohtaro and Barmina, Olga and Thompson, Ammon and Massey,
   Jonathan H. and Kim, Bernard Y. and Suvorov, Anton and Kopp, Artyom},
Title = {Evolution and development of male-specific leg brushes in Drosophilidae},
Journal = {DEVELOPMENT GENES AND EVOLUTION},
