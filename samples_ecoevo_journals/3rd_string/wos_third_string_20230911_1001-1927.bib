@article{ WOS:000755441200001,
Author = {Yan, Rong and Sun, Qiwen},
Title = {Uniqueness and stability of periodic solutions for an interactive wild
   and Wolbachia-infected male mosquito model},
Journal = {JOURNAL OF BIOLOGICAL DYNAMICS},
