@article{ WOS:000792558300001,
Author = {Leite, Maria C. A. and Chen-Charpentier, Benito and Agusto, Folashade B.
   and Gaoue, Orou G. and Hritonenko, Natali},
Title = {Viability of Pentadesma in reduced habitat ecosystems within two
   climatic regions with fruit harvesting},
Journal = {JOURNAL OF BIOLOGICAL DYNAMICS},
Year = {2022},
Volume = {16},
Number = {1},
Pages = {207-235},
Month = {DEC 31},
Abstract = {Habitat loss and harvesting of non-timber forest products (NTFPs)
   significantly affect the population dynamics. In this paper, we propose
   a general mathematical modelling approach incorporating the impact of
   habitat size reduction and non-lethal harvesting of NTFP on population
   dynamics. The model framework integrates experimental data of Pentadesma
   butyracea in Benin. This framework allows us to determine the rational
   non-lethal harvesting level and habitat size to ensure the stability of
   the plant ecosystem, and to study the impacts of distinct levels of
   humidity. We suggest non-lethal harvesting policies that maximize the
   economic benefit for local populations.},
