@article{ WOS:000739647800010,
Author = {Hernandez, Daniel L. and Bump, Joseph K.},
Title = {Predation of a Beaver (Castor canadensis) by a Gray Wolf (Canis lupus)
   during Winter},
Journal = {AMERICAN MIDLAND NATURALIST},
Year = {2022},
Volume = {187},
Number = {1},
Pages = {97-103},
Month = {JAN},
Abstract = {Wolf predation of beaver is common in boreal ecosystems, but predation
   events are thought to be rare in winter. We describe the encounter of a
   recent wolf predation event of a beaver during conditions of ice cover.
   The beaver had a broken upper incisor, which may have contributed to it
   foraging on land during the winter. While it is common for injured
   animals to have increased vulnerability to predation, injuries can also
   influence foraging decisions that may indirectly increase vulnerability
   to predation.},
