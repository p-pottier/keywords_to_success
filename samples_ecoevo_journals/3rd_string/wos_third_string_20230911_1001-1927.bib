   Blackwell, Bradley F.; Buckingham, Bruce N.; Pfeiffer, Morgan B., Natl Wildlife Res Ctr, US Dept Agr Anim \& Plant Hlth Inspect Serv, Wildlife Serv, 6100 Columbus Ave, Sandusky, OH 44870 USA.},
ISSN = {1934-4392},
EISSN = {1936-8046},
