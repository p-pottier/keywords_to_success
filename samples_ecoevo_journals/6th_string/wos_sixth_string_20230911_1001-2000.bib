@article{ WOS:000702662600005,
Author = {Kucherov, S. E.},
Title = {Identification of Calendar Years with Late Spring Frosts on the Basis of
   Anatomical Structure of Annual Rings of the Common Oak on the Zilair
   Plateau (Southern Urals)},
Journal = {RUSSIAN JOURNAL OF ECOLOGY},
Year = {2021},
Volume = {52},
Number = {5},
Pages = {383-390},
Month = {SEP},
Abstract = {In the southern tip of the Urals, late spring frosts, which regularly
   damage the leaves of the common oak (Quercus robur L.), are reflected in
   the formation of a specific anatomical structure of annual rings. The
   identification of calendar years with damage to oak leaves by late
   spring frosts, carried out on the basis of the known and revealed
   features of the anatomical structure of wood in annual rings of oak,
   showed that over the past 250 years, damage to leaves by frost most
   often occurred in the southern part of the Zilair plateau.},
