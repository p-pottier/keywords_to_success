@article{ WOS:000736720800014,
Author = {Madushani, K. G. P. and Athukorala, A. D. S. N. P. and Gama-Arachchige,
   N. S.},
Title = {Correlation of Cyanobacteria and Green Microalgae Richness with
   Physicochemical Parameters in Geothermal Springs},
Journal = {RUSSIAN JOURNAL OF ECOLOGY},
Year = {2021},
Volume = {52},
Number = {6},
Pages = {543-555},
Month = {NOV},
Abstract = {Algal diversity of geothermal springs in Sri Lanka is poorly studied and
   remains largely unknown. In this study Temperature, Conductivity, pH,
   Dissolved Oxygen and Biological Oxygen Demand of Wahawa, Madunagala,
   Maha Oya and Kapurella geothermal springs have been measured using
   standard methods. Water samples with algal mats have been collected from
   each spring and species of cyanobacteria and green microalgae have been
   identified using microscopic characteristics. The correlation of
   cyanobacteria and green microalgae richness with measured
   physicochemical parameters has been statistically analysed. 25
   cyanobacteria and green microalgae species have been identified from all
   sites. The lowest species richness has been observed in Kapurella and
   Madunagala with the highest temperature (67.3 +/- 0.1 degrees C) and the
   conductivity (8475 +/- 15 mu S) respectively which indicates a weak
   negative correlation of temperature and conductivity with species
   richness. The highest species richness has been recorded in Wahawa with
   relatively low temperature and conductivity. Species richness has been
   observed to correlate more highly with temperature and conductivity
   compared to other measured physicochemical parameters. The richness of
   cyanobacteria is higher than green microalgae, meaning cyanobacteria
   might adapt well to existing physicochemical parameters in thermal
   habitats. Phormidium sp., Calothrix sp. and Gloeocapsa sp. have been
   found to be in several springs and they can survive under broad range of
   physicochemical parameters. Nephrocytium sp. is a new record to
   geothermal springs which is unique to Kapurella spring. Nevertheless,
   there are no significant distribution patterns of cyanobacteria and
   green microalgae in relation to the evaluated physicochemical parameters
   measured separately, and the obtained results might have been caused by
   the combination of all environmental factors. Considerable species
   richness has been observed despite the extreme conditions.},
