@article{ WOS:000777400600007,
Author = {Ryzhanovskii, V. N. and Ryabitsev, V. K. and Gilev, V, A.},
Title = {Latitudinal Features of Nest Success of Passerine Birds (Passeriformes)
   in the Ob Forest Tundra and Yamal Peninsula},
Journal = {RUSSIAN JOURNAL OF ECOLOGY},
Year = {2022},
Volume = {53},
Number = {2},
Pages = {123-127},
Month = {APR},
Abstract = {Success in egg incubation and chick rearing in 18 passerine bird species
   has been analyzed based on the results of observations on the survival
   of 12 610 eggs in 2083 nests found in the Ob forest tundra and the
   southern, central, and northern Yamal peninsula. It has been found that
   a northward increase in nest success is observed in the direction from
   the Southern Urals and Kazakhstan to Yamal but not from the Baltic Sea
   to Yamal. In the total forest tundra-subarctic tundra space, nest
   success increases to the north in one group of species (the common
   redpoll, Lapland longspur, and common chiffchaff) but decreases in
   another group (the horned lark, red-throated pipit, bluethroat, and
   willow warbler).},
