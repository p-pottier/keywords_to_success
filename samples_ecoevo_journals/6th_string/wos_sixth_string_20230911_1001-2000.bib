@article{ WOS:000815785300026,
Author = {Lu, Yi-Fei and Xie, Wen-Yuan and Li, Hua-Dong and Chen, Zheng-Hai and
   Jin, Xiao-Feng},
Title = {Artemisia lanaticapitula (Asteraceae: Tribe Anthemideae), a New Species
   from Zhejiang, East China},
Journal = {SYSTEMATIC BOTANY},
Year = {2022},
Volume = {47},
Number = {2},
Pages = {567-574},
Month = {APR-JUN},
Abstract = {Artemisia lanaticapitula (Asteraceae: Anthemideae), a new species from
   Zhejiang in East China, is described and illustrated, based on the
   analyses of morphological observations and molecular phylogenetic
   evidence. This new species is similar to Artemi,ia anomala in having
   simple and undivided leaves, but differs by having phyllaries, corolla
