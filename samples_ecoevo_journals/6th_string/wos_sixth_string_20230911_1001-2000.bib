@article{ WOS:000680540300005,
Author = {Permyakov, P. A. and Trukhin, A. M.},
Title = {The Anthropogenic Impact on the True Seals (Carnivora: Phocidae) at the
   Mouth of Piltun Bay (Sakhalin Island, Russia)},
Journal = {RUSSIAN JOURNAL OF ECOLOGY},
Year = {2021},
Volume = {52},
Number = {4},
Pages = {290-295},
Month = {JUL},
Abstract = {A study of the anthropogenic impacts on the on-shore aggregation of true
   seals was carried out at a haul-out located at the mouth of Piltun Bay
   (the northeastern part of Sakhalin Island, Russia). During the ice-free
   period, three species of ice-associated seals inhabited the haul-out:
   bearded seals, ringed seals, and spotted seals. The total observation
   period was 492 days, from June through October of 2014-2017. During this
   time, 2445 cases of people and vehicles appearing near the haul-out were
   recorded, of which 514 led to disturbing the seals. Seven main factors
   of anthropogenic disturbance were identified and the degree of
   disturbance caused by each factor was estimated.},
