@article{ WOS:000658244400001,
Author = {Brozou, Anastasia and Fuller, Benjamin T. and Grimes, Vaughan and
   Lynnerup, Niels and Boldsen, Jesper L. and Jorkov, Marie Louise and
   Pedersen, Dorthe Dangvard and Olsen, Jesper and Mannino, Marcello A.},
Title = {Leprosy in medieval Denmark: Exploring life histories through a
   multi-tissue and multi-isotopic approach},
Journal = {AMERICAN JOURNAL OF PHYSICAL ANTHROPOLOGY},
Year = {2021},
Volume = {176},
Number = {1},
Pages = {36-53},
Month = {SEP},
Abstract = {Objectives By focusing on two Danish leprosaria (N AE stved and Odense;
   13th-16th c. CE) and using diet and origin as proxies, we follow a
   multi-isotopic approach to reconstruct life histories of patients and
   investigate how leprosy affected both institutionalized individuals and
   the medieval Danish community as a whole.
   Materials and Methods We combine archaeology, historical sources,
   biological anthropology, isotopic analyses (delta C-13, delta N-15,
   delta S-34, Sr-87/Sr-86) and radiocarbon dating, and further analyze
   bones with different turnover rates (ribs and long bones).
   Results The delta C-13, delta N-15 and delta S-34 results indicate a C-3
   terrestrial diet with small contributions of marine protein for leprosy
   patients and individuals from other medieval Danish sites. A similar
   diet is seen through time, between males and females, and patients with
   and without changes on facial bones. The isotopic comparison between
   ribs and long bones reveals no significant dietary change. The delta
   S-34 and Sr-87/Sr-86 results suggest that patients were local to the
   regions of the leprosaria. Moreover, the radiocarbon dates show a mere
   50\% agreement with the arm position dating method used in Denmark.
   Conclusions A local origin for the leprosy patients is in line with
   historical evidence, unlike the small dietary contribution of marine
   protein. Although only 10\% of the analyzed individuals have rib/long
   bone offsets that undoubtedly show a dietary shift, the data appear to
   reveal a pattern for 25 individuals (out of 50), with elevated delta
   C-13 and/or delta N-15 values in the ribs compared to the long bones,
   which points toward a communal type of diet and reveals organizational
   aspects of the institution.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
