@article{ WOS:000807493900007,
Author = {Smirnov, N. G. and Kropacheva, Yu. E.},
Title = {Time Scales of the Dynamics of Rodent Communities and their Alignment
   with the Levels of Spatial Differences between Faunas},
Journal = {RUSSIAN JOURNAL OF ECOLOGY},
Year = {2022},
Volume = {53},
Number = {3},
Pages = {204-211},
Month = {JUN},
Abstract = {This work involved materials from the recent and ancient (Holocene and
   Late Pleistocene) localities of the ornithogenic assemblages of Ural
   rodent bone remains. The work included the development and trial of
   criteria for assignment of the temporal dynamics of rodent communities
   to particular scales based on their comparison against levels of the
   spatial differences of the recent rodent faunas from localities situated
   in the territories of the various phytogeographical ranks, namely,
   districts, subzones, and zones. Paired coefficients of the ecological
   and taxonomic similarities were calculated based on a distribution of
   proportions of the rodent remains (\%) in heterochronous localities of
   the Middle Urals. The paper demonstrates analogy between spatial
   differences and temporal dynamics: the zone differences-evolutionary
   scale, subzone differences-historical scale, and intradistrict
   differences-actual scale.},
