@article{ WOS:000736720800015,
Author = {Gurskaya, M. A.},
Title = {Formation of Frost Damage in Larch Growing in the Northern and Southern
   Forest-Tundra Subzones of Siberia: A Comparative Analysis},
Journal = {RUSSIAN JOURNAL OF ECOLOGY},
Year = {2021},
Volume = {52},
Number = {6},
Pages = {556-566},
Month = {NOV},
Abstract = {Global climate change has led to shifts in the frequency and severity of
   extreme eather and climatic events, including the occurrence of
   extremely low temperatures during the growing period, which results in
   frost damage to living xylem cells. In this study, the frequency of
   frost damages in larch has been assessed along a 4000-km transect
   through the northern and southern forest-tundra subzones of the Siberian
   Subarctic. The results show that frost damage are more frequent in trees
   growing in in the southern forest-tundra and river valleys than in the
   northern subzone. Probable factors responsible for this phenomenon are
   discussed (tree-ring width, the onset and end dates of the growing
   period, the minimum air temperature and the amplitude of temperatures
   during the growing period).},
