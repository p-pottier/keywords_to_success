@article{ WOS:000660551200006,
Author = {Zorina, A. A. and Shuiskaya, E. A.},
Title = {Analysis of the Shifts of Phenological Events in Birds as Indicators of
   Climatogenic Changes},
Journal = {RUSSIAN JOURNAL OF ECOLOGY},
Year = {2021},
Volume = {52},
Number = {3},
Pages = {230-237},
Month = {MAY},
Abstract = {A new method for assessing the response of biota to climatic changes in
   the ecosystem has been tested. In the Central Forest Natural Biosphere
   Reserve, the average annual air temperature between 1990 and 2019 was
   4.8 +/- 0.13 degrees C (increasing at a rate of 0.35 degrees C per 10
   years), and the amount of precipitation was 781 +/- 22.9 mm/year (22.6
   mm per 10 years). A significant change in the timing of the seasonal
   activity has been determined for 11 phenological phenomena in nine bird
   species (52\%). The clustering of phenological events in birds depends
   on the pattern of their seasonal migrations, the effect of agro- and
   hydrometeorological and trophic factors, and on whether spring or autumn
   phenological dates are used. Along with multivariate statistical
   methods, the integral index based on the standardized deviation makes it
   possible to unify the meteorological series and phenological traits on a
   biologically and statistically sound basis for improving the sensitivity
   of indicative properties of the biota under conditions of climatogenic
   changes.},
