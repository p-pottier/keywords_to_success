@article{ WOS:000815776200015,
Author = {Ferm, Julia and Stahl, Bertil},
Title = {A Revised Subgeneric Classification of the Neotropical Genus Zapoteca
   (Caesalpinioideae; Fabaceae)},
Journal = {SYSTEMATIC BOTANY},
Year = {2022},
Volume = {47},
Number = {1},
Pages = {208-214},
Month = {JAN-MAR},
Abstract = {The subgeneric classification of the neotropical legume genus Zapoteca
   is revised based on an analysis of the nuclear ETS and ITS, and the
   plastid trnL-trnF DNA regions. Zapoteca subgenus Ravenia is included in
   Z. subgenus Zapoteca, Z. quichoi is transferred from Z. subgenus
   Amazonica to Z. subgenus Zapoteca, Z. sousae is included in Z. subgenus
   Zapoteca, and the new subgenus Zapoteca subgenus Subamazonica is
   described to accommodate the species Z. filipes and Z. scutellifera. A
   key to the subgenera is presented.},
