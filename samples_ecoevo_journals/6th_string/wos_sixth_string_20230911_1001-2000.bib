@article{ WOS:000687219800016,
Author = {Manchester, Steven R. and Lott, Terry A. and Herrera, Fabiany and
   Hooghiemstra, Henry and Wijninga, Vincent M. and Fritsch, Peter W.},
Title = {Symplocos Fruits from the Pliocene of Colombia},
Journal = {SYSTEMATIC BOTANY},
Year = {2021},
Volume = {46},
Number = {2},
Pages = {416-421},
Month = {APR-JUN},
Abstract = {Fossil fruits of Symplocos (Ericales: Symplocaceae) are here recognized
   from the Pliocene of Guasca, Colombia, based on specimens formerly
   attributed to Cordia (Cordiaceae, Boraginales). Symplocos vera (Berry)
   comb. nov. is represented by 19 lignitized fruits. The fossils are
   recognized as belonging to Symplocos primarily by their woody endocarps
   that are apically truncate and that possess 3 to 5 apical germination
   pores and locules, and a central vascular canal extending the length of
   the endocarp. In several key characters they are highly congruent with
   the endocarps of the extant Neotropical clade S. ser. Symplocos. Some of
   the extant species in the series are variably 3- to 5-locular; 4-locular
   endocarps are otherwise rare in Symplocos, and 5-locular endocarps
   appear to be unique to this series. Symplocos vera is the only
   specifically named record of fossil Symplocos fruits with accessible
   voucher specimens from South America. The younger Neogene age of the
   fossils relative to those attributed to S. ser. Symplocos from the late
   Eocene of Texas, along with a report of Colombian fossil endocarps from
   the middleMiocene, supports the North America to South America migration
   inferred for this clade from molecular phylogenetic data.},
