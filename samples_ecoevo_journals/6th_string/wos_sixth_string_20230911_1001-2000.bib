@article{ WOS:000736720800002,
Author = {Shirokikh, P. S. and Yusupova, V, O. and Abramova, L. M. and Naumova, L.
   G.},
Title = {Ecological Characteristics of Anemonastrum biarmiense (Juz.) Holub in
   the Southern Urals},
Journal = {RUSSIAN JOURNAL OF ECOLOGY},
Year = {2021},
Volume = {52},
Number = {6},
Pages = {438-445},
Month = {NOV},
Abstract = {An ecologo-floristic approach and Tsyganov's ecological indicator values
   have been used to analyze the effect of abiotic factors on the
   distribution of Anemonastrum biarmiense (Juz.) Holub, a species endemic
   to the Southern Urals. The results show that this species has a wide
   range of tolerance to edaphic factors, with its synecological optimum
   being positioned centrally. Therefore, changes in edaphic conditions
   have no significant effect on its productivity and vitality. With
   respect to climatic factors, the realized ecological niche of A.
   biarmiense is narrow. The synecological optimum is displaced to its
   boundary and corresponds to mountain tundra communities, where habitat
   conditions change over relatively small distances. Therefore, the
   response of A. biarmiense to climate warming will involve in the first
   place a decrease in the density of cenopopulations with the following
   reduction of habitats. Thus, the combination of the narrow range of
   tolerance to climatic factors and the position of synecological optimum
   at the boundary of realized ecological niche makes A. biarmiense a good
   indicator of global climate change.},
