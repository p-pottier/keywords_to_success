@article{ WOS:000713767700020,
Author = {Lucas, Zohara and Pulsford, Stephanie and Wimpenny, Claire and Snape,
   Melissa},
Title = {Seasonal breeding of the Eastern Grey Kangaroo provides opportunities
   for improved animal welfare in kangaroo management},
Journal = {ECOLOGICAL MANAGEMENT \& RESTORATION},
Year = {2021},
Volume = {22},
Number = {1, SI},
Pages = {138-140},
Month = {NOV},
Abstract = {The challenge of maintaining biodiversity by managing overabundant
   native fauna is often a contentious issue. This study demonstrates a
   means of improving animal welfare in the culling of overabundant
   kangaroos. We studied five populations of Eastern Grey Kangaroo
   (Macropus giganteus) in the Australian Capital Territory and established
   the percentage of juveniles throughout the year. We focused on juveniles
   aged 8-12 months old, as these are identified at greatest risk of poor
   welfare outcomes if orphaned. Our findings support the timing of culls
   when the fewest vulnerable juveniles are present in the population. This
   paper provides a scientific basis for land managers to adjust the timing
   of kangaroo management activities to best balance animal welfare, land
   management and conservation objectives.},
