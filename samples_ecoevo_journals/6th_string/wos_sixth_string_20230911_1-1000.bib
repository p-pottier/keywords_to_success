@article{ WOS:000635718100001,
Author = {Wrotek, Sylwia and LeGrand, Edmund K. and Dzialuk, Artur and Alcock, Joe},
Title = {Let fever do its job The meaning of fever in the pandemic era},
Journal = {EVOLUTION MEDICINE AND PUBLIC HEALTH},
Year = {2021},
Volume = {9},
Number = {1},
Pages = {26-35},
Abstract = {Although fever is one of the main presenting symptoms of COVID-19
   infection, little public attention has been given to fever as an evolved
   defense. Fever, the regulated increase in the body temperature, is part
   of the evolved systemic reaction to infection known as the acute phase
   response. The heat of fever augments the performance of immune cells,
   induces stress on pathogens and infected cells directly, and combines
   with other stressors to provide a nonspecific immune defense.
   Observational trials in humans suggest a survival benefit from fever,
   and randomized trials published before COVID-19 do not support fever
   reduction in patients with infection. Like public health measures that
   seem burdensome and excessive, fevers involve costly trade-offs but they
   can prevent infection from getting out of control. For infections with
   novel SARS-CoV-2, the precautionary principle applies: unless evidence
   suggests otherwise, we advise that fever should be allowed to run its
   course.},
