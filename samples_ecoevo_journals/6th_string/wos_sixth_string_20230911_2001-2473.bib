@article{ WOS:000722924900008,
Author = {Ibisch, Pierre L. and Dichte, Angela and Hamor, Fedir D. and Holovko,
   Andrii and Pokynchereda, Vasyl F. and Rybak, Mykola P. and Shvediuk,
   Iuliia},
Title = {About borders and limits: experiences with UNESCO Biosphere Reserves for
   trans-boundary cooperation in Ukraine and neighbouring countries},
Journal = {ECO MONT-JOURNAL ON PROTECTED MOUNTAIN AREAS RESEARCH},
Year = {2021},
Volume = {13},
Number = {SI},
Pages = {136-141},
Month = {NOV},
Abstract = {In Eastern Europe, several UNESCO Biosphere Reserves (BRs) have been
   important promoters of transnational and transboundary nature
   conservation and sustainable development. Nevertheless, political and
   institutional barriers and scarcity of resources have hindered
   successful cross-border cooperation. The prospects for BRs in Eastern
   Europe are mixed, especially in relation to the challenges of ecosystem
   degradation and climate change, as well as socioeconomic and
   (geo)political crises. Based on our own experience of one and a half
   decades of growing cooperation between partners in Ukraine and Germany,
   we conclude that transnational cooperation between BRs and science is
   particularly rewarding and mutually beneficial.},
