@article{ WOS:000878413300003,
Author = {Depa, Lukasz and Vepsalainen, Kari and Rutkowski, Tomasz and Czechowski,
   Wojciech},
Title = {Mutualism between Lasius ant species and Stomaphis aphids in relation to
   social parasitism among the ants},
