@article{ WOS:000641296900001,
Author = {Davis, Hayden R. and Das, Indraneil and Leache, Adam D. and Karin,
   Benjamin R. and Brennan, Ian G. and Jackman, Todd R. and Nashriq, Izneil
   and Onn Chan, Kin and Bauer, Aaron M.},
Title = {Genetically diverse yet morphologically conserved: Hidden diversity
   revealed among Bornean geckos (Gekkonidae: Cyrtodactylus)},
Journal = {JOURNAL OF ZOOLOGICAL SYSTEMATICS AND EVOLUTIONARY RESEARCH},
Year = {2021},
Volume = {59},
Number = {5},
Pages = {1113-1135},
Month = {JUL},
Abstract = {The appreciation of cryptic biological diversity, and the pace at which
   it is recognized, has greatly increased with the use of molecular
   systematic techniques. The gekkonid genus Cyrtodactylus Gray, 1827 is
   one example of a group that has undergone a particularly rapid increase
   in recognized diversity due to molecular systematic studies. Many of
   these new species result from recognizing closely related but
   diagnosable lineages into sister taxa. Our study implements a
   multi-faceted approach to delimit cryptic Cyrtodactylus lineages on the
   Southeast Asian island of Borneo using morphological, ecological, and
   multilocus genetic data. We use multiple species delimitation models to
   assess species boundaries and identify clades that warrant further
   investigation. Unlike most morphologically cryptic species that have
   recently diverged, we find evidence of cryptic lineages being
   polyphyletic. Using multivariate statistical analyses, we show minimal
   phenotypic distinction between putative cryptic species within the C.
   pubisulcus complex. Despite not finding morphologically diagnostic
   characters, we demonstrate strong evidence for the specific recognition
   of C. hantu sp. nov. and C. miriensis sp. nov., which are currently
   considered conspecific with C. pubisulcus, from Sarawak, Malaysia. Our
   new concept for C. pubisulcus restricts the geographic range of the
   species to specific regions in western Sarawak, Malaysia, thus
   underscoring the need to conserve the limited remaining habitats of
   these species, as well as the considerable undescribed diversity across
   Borneo.},
