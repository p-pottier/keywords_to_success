@article{ WOS:000625308500009,
Author = {Sokol, Miloslawa},
Title = {Impact of population size at a rate of morphological and molecular
   evolution - the use of an object-oriented model},
Journal = {ISRAEL JOURNAL OF ECOLOGY \& EVOLUTION},
Year = {2021},
Volume = {67},
Number = {1-2},
Pages = {81-91},
Month = {FEB},
Abstract = {A generalization of Moran model of evolution is created using
   object-oriented method of modelling. A population consists of
   individuals which have a genotype and a phenotype. The genotype is
   inherited by descendants and it can mutate. The phenotype is dependent
   on the genotype. Moreover, the phenotype causes changes in the fitness
   of the individuals (natural selection which four kinds are defined and
   analysed). Evolution of the population appears spontaneously. This model
   is used to analyse how population size influence the rate of evolution.
   Evolution is manifested by two processes: the increase of the phenotype
   size (morphological evolution) and number of mutations accumulated on
   genes (molecular evolution). The rate of evolution increases if
   population size increases. An adaptive natural selection causes
   nonlinear changes in the phenotype size and number of mutations
   accumulated on genes. A competitive natural selection causes linear
   evolution. A surviving natural selection causes the faster evolution
   than a reproductive natural selection.},
