   Ebert, Dieter, Univ Basel, Zool, Dept Environm Sci, Vesalgasse 1, CH-4051 Basel, Switzerland.},
DOI = {10.1186/s13227-022-00199-0},
Article-Number = {16},
EISSN = {2041-9139},
Keywords = {Daphnia magna; Daphnia pulex; Cladocera; Branchiopoda; Cyclic
