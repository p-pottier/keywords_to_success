@article{ WOS:000837681700001,
Author = {Ebert, Dieter},
Title = {Daphnia as a versatile model system in ecology and evolution},
Journal = {EVODEVO},
Year = {2022},
Volume = {13},
Number = {1},
Month = {AUG 8},
Abstract = {Water fleas of the genus Daphnia have been a model system for hundreds
   of years and is among the best studied ecological model organisms to
   date. Daphnia are planktonic crustaceans with a cyclic parthenogenetic
   life-cycle. They have a nearly worldwide distribution, inhabiting
   standing fresh- and brackish water bodies, from small temporary pools to
   large lakes. Their predominantly asexual reproduction allows for the
   study of phenotypes excluding genetic variation, enabling us to separate
   genetic from non-genetic effects. Daphnia are often used in studies
   related to ecotoxicology, predator-induced defence, host-parasite
   interactions, phenotypic plasticity and, increasingly, in evolutionary
   genomics. The most commonly studied species are Daphnia magna and D.
   pulex, for which a rapidly increasing number of genetic and genomic
   tools are available. Here, I review current research topics, where the
   Daphnia model system plays a critical role.},
