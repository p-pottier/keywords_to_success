@article{ WOS:000971259800017,
Author = {Strong, Cheryl and Neuman, Kriss K. and Hutchinson, Jenny L. and Miller,
   Jamie K. and Clark, Amber L. and Chang, Lena and Iwanicha, Joanna and
   Feucht, Elizabeth and Lau, Matthew J. and Lauten, David J. and
   Markegard, Sarah and Pearl, Benjamin and Sherer, David L. and Tertes,
   Rachel and Tharratt, Susie and Wooten, Travis},
Title = {Common raven impacts on nesting western snowy plovers: integrating
   management to facilitate species recovery},
Journal = {HUMAN-WILDLIFE INTERACTIONS},
Year = {2021},
Volume = {15},
Number = {3},
Pages = {465-478},
Month = {WIN},
Abstract = {The U.S. Pacific coast population of the western snowy plover
   (Charadrius nivosus nivosus; plover) has declined due to loss and
   degradation of coastal habitats, predation, and anthropogenic
   disturbance. The U.S. Fish and Wildlife Service listed the subspecies in
   1993 as threatened under the Endangered Species Act due to the
   population declines and habitat loss. Predation of nests and chicks has
   been identified as an important cause of historic population declines,
   and thus, most predator management actions for this subspecies are
   focused on reducing this pressure. In recent years, common ravens
   (Corvus corax; ravens) have become the most common and pervasive
   predators of plover nests and chicks, especially in areas with
   subsidized food sources for ravens and sites without predator
   management. We compiled data from a variety of sources to document the
   impact of raven predation on plover nesting success. We discuss current
   raven management and suggest several tools and strategies to increase
   plover nesting success, including multi-state approval for the use of
   the avicide DRC-1339, the use of lures and new trap types, and an
   increase in funding for predator management. The lack of coordinated and
   integrated management continues to impede the recovery of the Pacific
   coast plover population.},
