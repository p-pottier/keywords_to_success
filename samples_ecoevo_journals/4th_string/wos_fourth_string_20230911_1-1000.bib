@article{ WOS:000657669100001,
Author = {Matsuda, Hiroshi and Higuchi, Hirokazu and Miyaji, Naoki and Okabe,
   Masanori},
Title = {Anatomical observations on pistil morphogenesis and fertilization in
   salak (Salacca wallichiana C. Mart.)},
Journal = {TROPICS},
Year = {2021},
Volume = {30},
Number = {1},
Pages = {1-9},
Abstract = {Salak palms (Salacca spp.) are important fruit species in Southeast
   Asia, but the morphogenesis of their reproductive organs is unknown. To
   observe female gametogenesis, pistil morphogenesis of S. wallichiana and
   subsequent fertilization were observed. The ovary contained three ovule
   primordia three months before flowering (BF). The ovule contained one
   mono-nucleate megaspore at the center and three degenerated megaspores
   on the chalazal side at two months BF, and a mononucleate embryo sac
   (ES) at one month BF. The nucellus enlarged, and protruded beak-like
   from the integuments, forming a micropylar structure. The ovule was
   anatropous, unitegmic, and crassinucellate. Three weeks BF, the ES was
   at the 8-nucleate stage. Two weeks BF, an egg apparatus comprising one
   egg-cell nucleus and two synergid nuclei were developing on the
   micropylar side. The central cell contained one polar nucleus (PN). One
   week BF, the synergids and central cell became multinucleate and
   multiple-PN dispersed in the ES. Several days BF, one PN was again
   observed, and the remaining PN appeared to develop cell groups along the
   ES wall. Anripodals existed on the chalazal side. One day BF, ES
   contained two PN, and one day after pollination (DAP), the zygote and
   primary endosperm nucleus occurred on the micropylar side. One synergid
   remained functional and enlarged even after fertilization. Four DAP and
   thereafter, suspensor cells and an early embryo existed on the
   micropylar side, and the ES contained multiple free endosperm nuclei. In
   S. wallichiana, the ES likely indicates intermediate morphogenesis
   between Polygonum and Oenothera types, suggesting a unique development
   pattern among palms.},
Publisher = {JAPAN SOC TROPICAL ECOLOGY},
Address = {C/O DR.T. IWAKUMA, GRADUATE SCHOOL ENV EARTH SCIENCE, HOKKAIDO UNIV,
