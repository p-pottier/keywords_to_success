@article{ WOS:000612234300002,
Author = {Uetz, Peter and Slavenko, Alex and Meiri, Shai and Heinicke, Matthew},
Title = {Gecko diversity: a history of global discovery},
Journal = {ISRAEL JOURNAL OF ECOLOGY \& EVOLUTION},
Year = {2020},
Volume = {66},
Number = {3-4, SI},
Pages = {117-125},
Month = {NOV},
Abstract = {1935 gecko species (and 224 subspecies) were known in December 2019 in
   seven families and 124 genera. These nearly 2000 species were described
   by similar to 950 individuals of whom more than 100 described more than
   10 gecko species each. Most gecko species were discovered during the
   past 40 years. The primary type specimens of all currently recognized
   geckos (including subspecies) are distributed over 161 collections
   worldwide, with 20 collections having about two thirds of all primary
   types. The primary type specimens of about 40 gecko taxa have been lost
   or unknown. The phylogeny of geckos is well studied, with DNA sequences
   being available for similar to 76\% of all geckos (compared to similar
   to 63\% in other reptiles) and morphological characters now being
   collected in databases. Geographically, geckos occur on five continents
   and many islands but are most species-rich in Australasia (which also
   houses the greatest diversity of family-level taxa), Southeast Asia,
   Africa, Madagascar, and the West Indies. Among countries, Australia has
   the highest number of geckos (241 species), with India, Madagascar, and
   Malaysia being the only other countries with more than 100 described
   species each. As expected, when correcting for land area, countries
   outside the tropics have fewer geckos.},
