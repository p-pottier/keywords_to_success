EISSN = {1616-1564},
Keywords = {Arctic marine environment; Carbon cycle; Colwellia psychrerythraea
   strain 34H; Enzymatic activity; Psychrophiles; Sea ice; Psychrobacter
   strain 7E},
Keywords-Plus = {DISSOLVED ORGANIC-MATTER; PSYCHRERYTHRAEA STRAIN 34H; SALT-TOLERANT
   ESTERASE; SURFACE MICROLAYER; SERINE-PROTEASE; SUBSTANCES EPS;
