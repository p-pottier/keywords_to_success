@article{ WOS:000806775300001,
Author = {Jungck, John R. and Cebeci, Metehan},
Title = {Wenxiang 3.0: Evolutionary Visualization of alpha, pi, and 3/10 Helices},
Journal = {EVOLUTIONARY BIOINFORMATICS},
Year = {2022},
Volume = {18},
Month = {MAY},
Abstract = {Wenxiang diagrams illustrate protein helices as spirals on a plane and
   thus have the advantage over helical wheels of being planar graphs.
   Wenxiang 3.0 extends the original version by adding 3 major features:
   (1) individual amino acid residues can be colored according to their
   evolutionary conservation in comparative multiple sequence alignments
   using CONSURF encoding; (2) alpha, pi, and 3/10 helices can be
   illustrated by overlaying arcs representative of the pitches of these
   helices; and, (3) the physico-chemical properties of amino acids
   residues in the protein sequence can be re-presented by colored
   geometric shapes.},
