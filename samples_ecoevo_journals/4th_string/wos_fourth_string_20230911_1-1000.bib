@article{ WOS:000605998600001,
Author = {Caballero-Vinas, Carmen and Sanchez-Nava, Petra and Aguilar-Ortigoza,
   Carlos and Morrone, Juan J.},
Title = {Dispersal of North American Polymorphidae (Acanthocephala) and aquatic
   birds (Anatidae and Rallidae) along the Central Migratory Flyway},
Journal = {JOURNAL OF ZOOLOGICAL SYSTEMATICS AND EVOLUTIONARY RESEARCH},
Year = {2021},
Volume = {59},
Number = {3},
Pages = {561-575},
Month = {APR},
Abstract = {Some species of parasites use highly vagile organisms as definitive
   hosts, and this may influence the geographic distribution and historical
   diversification of both hosts and parasites, shaped by common
   biogeographic processes. Acanthocephalans are a group of endoparasites,
   some of which use highly vagile migratory birds as definitive hosts. The
   objective of the present work was to determine the evolutionary and
   biogeographic events shared by these taxa along the North American
   Central Migratory Flyway. A database with the records of Anatid and
   Rallid species and their acanthocephalans was compiled using previous
   publications and collections. Tanglegrams were constructed using the
   method of reconciled trees, and a general cladogram was constructed
   using the Brooks parsimony analysis. The results suggested that there is
   no cospeciation between the acanthocephalans and aquatic birds, and
   biotic evolution is shaped mainly by vicariance events for birds and
   dispersal events for acanthocephalans. Barriers seem to have been
   overcome by acanthocephalans but not by their hosts, placing them as
   generalist parasites and successful colonizers.},
