@article{ WOS:000651569600001,
Author = {Ribeiro-Junior, Jr., Marco Antonio and Sanchez-Martinez, Paola Maria and
   Carneiro de Lima Moraes, Leandro Joao and de Oliveira, Uecson Suendel
