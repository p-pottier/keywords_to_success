@article{ WOS:000621233800015,
Author = {Angulo-Preckler, Carlos and Garcia-Lopez, Eva and Figuerola, Blanca and
   Avila, Conxita and Cid, Cristina},
Title = {Natural chemical control of marine associated microbial communities by
