@article{ WOS:000625308500003,
Author = {Moroenyane, I and Tripathi, B. M. and Adams, J. M. and Chen, S. and
   Steinberger, Y.},
Title = {Slope aspect influences soil microbial community structure and
   composition in the Israel arid Mediterranean},
