@article{ WOS:000621233800009,
Author = {Tsuchiya, Kenji and Sano, Tomoharu and Tomioka, Noriko and Kazuhiro,
   Komatsu and Imai, Akio and Hayakawa, Kazuhide and Nagata, Takamaru and
   Okamoto, Takahiro and Kuwahara, Victor S. and Kohzu, Ayato},
Title = {Novel method for measuring aquatic bacterial productivity using
   D-10-leucine based on protein synthesis rate},
Journal = {AQUATIC MICROBIAL ECOLOGY},
Year = {2020},
Volume = {85},
Pages = {121-129},
Abstract = {The most widely used method for measuring bacterial production is
   tritium-labeled leucine (H-3-Leu). Although this method provides
   methodological simplicity and high sensitivity, the employment of
   radioactive isotopes is often restricted by regulations, particularly in
   field settings. In this study, we developed a non-radioactive method for
   measuring bacterial productivity based on the protein synthesis rate,
   using deuterium-labeled leucine ((CD3)(2)CDCD2CD(NH2)COOH; D-10-Leu);
   the proposed method was then compared and verified with the H-3-Leu
   method. The procedures of the proposed method are (1) incorporation of
   D-10-Leu by bacteria, (2) acid hydrolysis (HCl) to amino acids and (3)
   quantification of D-10-Leu (m/z 142.10) by liquid chromatography mass
   spectrometry (LC-MS/MS). In the LC-MS/MS analysis, we detected a larger
   amount of D-9-Leu (m/z 141.10) and D-8-Leu (m/z 140.10) than that of
   D-10-Leu, suggesting that incorporated D-10-Leu was rapidly metabolized
   such as in deamination and aminotransferase reactions. The incorporation
   rates of D-10-Leu, D-10-Leu + D-9-Leu (D-10+D-9-Leu) and D-10-Leu +
   D-9-Leu + D-8-Leu (D-10+D-9+D-8-Leu) were significantly positively
   correlated to that of H-3-Leu, confirming the validity of the proposed
   method. Since D-7-Leu (m/z 139.10) could not be detected, the amount of
   exogenous leucine incorporated into protein can be accurately estimated
   through D-10+D-9+D-8-Leu measurement. The new compound-based
   quantification method using stable isotope-labeled leucine can be a
   powerful tool to estimate pure protein synthesis rate for measuring
   bacterial production.},
