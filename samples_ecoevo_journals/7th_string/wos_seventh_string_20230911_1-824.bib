@article{ WOS:000561794300005,
Author = {Ding, Anyang and Pittman, Michael and Upchurch, Paul and O'Connor,
   Jingmai and Field, Daniel J. and Xu, Xing},
Title = {The Biogeography of Coelurosaurian Theropods and Its Impact on Their
   Evolutionary History},
Journal = {BULLETIN OF THE AMERICAN MUSEUM OF NATURAL HISTORY},
