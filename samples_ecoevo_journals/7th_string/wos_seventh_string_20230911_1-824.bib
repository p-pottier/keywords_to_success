@article{ WOS:000517756000001,
Author = {Nie, Yuan and Foster, Charles S. P. and Zhu, Tianqi and Yao, Ru and
   Duchene, David A. and Ho, Simon Y. W. and Zhong, Bojian},
Title = {Accounting for Uncertainty in the Evolutionary Timescale of Green Plants
   Through Clock-Partitioning and Fossil Calibration Strategies},
Journal = {SYSTEMATIC BIOLOGY},
