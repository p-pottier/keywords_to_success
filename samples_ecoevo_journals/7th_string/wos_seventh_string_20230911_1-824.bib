@article{ WOS:000493314500007,
Author = {Borowiec, Marek L.},
Title = {Convergent Evolution of the Army Ant Syndrome and Congruence in Big-Data
   Phylogenetics},
Journal = {SYSTEMATIC BIOLOGY},
