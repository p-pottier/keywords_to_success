Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Liu, YT (Corresponding Author), Beijing Jiaotong Univ, Dept Math, Beijing 100044, Peoples R China.
   Yang, ZH (Corresponding Author), UCL, Dept Genet Evolut \& Environm, Gower St, London WC1E 6BT, England.
   Zhu, TQ (Corresponding Author), Chinese Acad Sci, Acad Math \& Syst Sci, Beijing 100190, Peoples R China.
