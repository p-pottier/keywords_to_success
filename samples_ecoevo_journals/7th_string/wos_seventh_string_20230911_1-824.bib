@article{ WOS:000433274900004,
Author = {Carvalho, Tiago P. and Lundberg, John G. and Baskin, Jonathan N. and
   Friel, John P. and Reis, Roberto E.},
Title = {A new species of the blind and miniature genus Micromyzon Friel and
   Lundberg, 1996 (Silurifomes: Aspredinidae) from the Orinoco River:
   describing catfish diversity using high-resolution computed tomography},
Journal = {PROCEEDINGS OF THE ACADEMY OF NATURAL SCIENCES OF PHILADELPHIA},
Year = {2016},
Volume = {165},
Pages = {37-53},
Month = {DEC},
Abstract = {A new species of the aspredinid catfish tribe Hoplomyzontini Micromyzon
   is described from two specimens collected with trawl nets in two
   localities, at 10 and 18 m depth, in the main channel of the lower
   Orinoco River in Venezuela almost 40 years ago. The new species is
   distinguished from its only congener, Micromyzon akamai, by the:
   straight anterior margin of the mesethmoid; open posterior cranial
   fontanel; ossified first pectoral-fin radial; single tubular
   infraorbital bone; infraorbital sensory canal entering neurocranium via
   the frontal; enclosed foramen for the abductor superficialis muscle in
   the coracoid; higher vertebral count (33 vs. 28-32); higher anal-fin ray
   count (10 or 11 vs. 7-9); and some morphometric features. The holotype
   of the new species was scanned using High-Resolution X-ray Computed
   Tomography to illustrate, describe, and compare its bony skeleton to
   other hoplomyzontins.},
