@article{ WOS:000489700900011,
Author = {Calisher, Charles H. and Briese, Thomas and Brister, J. Rodney and
   Charrel, Remi N. and Duerrwald, Ralf and Ebihara, Hideki and Fulhorst,
   Charles F. and Gao, George Fu and Groschup, Martin H. and Haddow, Andrew
