@article{ WOS:000448779100001,
Author = {Schwartz, Michael D. and Weirauch, Christiane and Schuh, Randall T.},
Title = {NEW GENERA AND SPECIES OF MYRTACEAE-FEEDING PHYLINAE FROM AUSTRALIA, AND
   THE DESCRIPTION OF A NEW SPECIES OF RESTIOPHYLUS (INSECTA: HETEROPTERA:
   MIRIDAE)},
Journal = {BULLETIN OF THE AMERICAN MUSEUM OF NATURAL HISTORY},
Year = {2018},
Number = {424},
Pages = {1-157},
Month = {OCT},
Abstract = {Six new genera of Australian Phylinae are described on the basis of
   existing collections. The tribe Exaeretini-represented by the two new
   genera Eucalyptophylus (two new species) and Melaleucaphylus (18 new
   species)-is recognized for the first time as occurring in Australia.
   Nine new taxa of Semiini, subtribe Exocarpocorina, are proposed: Four
   new genera, Calytriphylus, Melaleucacoris, Teddus (each monotypic), and
