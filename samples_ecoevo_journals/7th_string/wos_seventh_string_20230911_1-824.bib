@article{ WOS:000623116700001,
Author = {Barta, Daniel E. and Norell, Mark A.},
Title = {THE OSTEOLOGY OF HAYA GRIVA (DINOSAURIA: ORNITHISCHIA) FROM THE LATE
   CRETACEOUS OF MONGOLIA},
