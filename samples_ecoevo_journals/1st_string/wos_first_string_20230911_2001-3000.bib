@article{ WOS:000877818400001,
Author = {Richter, Daniel D.},
Title = {Past critique but much praise for restoration ecology: four decades on},
Journal = {RESTORATION ECOLOGY},
Year = {2022},
Month = {2022 OCT 23},
Abstract = {During the last four decades, restoration ecology has prevailed and
   flourished in the academy and the public square. During this time, both
   ecology and restoration ecology have moved away from presenting
   ecosystem development as a trajectory that can return to
   ``pre-disturbance original condition.{''} Evolutionary change is a much
   better descriptor for ecosystem development as ecology and restoration
   ecology enter the Anthropocene.},
