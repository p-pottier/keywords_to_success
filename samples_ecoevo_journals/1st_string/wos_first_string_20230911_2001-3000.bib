@article{ WOS:000884497600001,
Author = {Asbridge, Emma and Dwyer, Patrick},
Title = {Marine Vegetation Management Strategies: a framework for estuary wide
   prioritization of protection and rehabilitation},
Journal = {RESTORATION ECOLOGY},
