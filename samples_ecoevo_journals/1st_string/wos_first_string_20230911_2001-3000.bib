@article{ WOS:000860125100001,
Author = {McGlothlin, Joel W. and Kobiela, Megan E. and Wright, Helen V. and
   Kolbe, Jason J. and Losos, Jonathan B. and Brodie III, Edmund D.},
Title = {Conservation and Convergence of Genetic Architecture in the Adaptive
   Radiation of Anolis Lizards},
Journal = {AMERICAN NATURALIST},
Year = {2022},
Volume = {200},
