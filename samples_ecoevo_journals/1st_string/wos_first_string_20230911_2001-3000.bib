@article{ WOS:000880001000005,
Author = {Wen, Zhonghua and Chen, Yixuan and Liu, Zunqi and Meng, Jun},
Title = {Biochar and arbuscular mycorrhizal fungi stimulate rice root growth
   strategy and soil nutrient availability},
Journal = {EUROPEAN JOURNAL OF SOIL BIOLOGY},
Year = {2022},
Volume = {113},
Month = {NOV-DEC},
Abstract = {The application of biochar and arbuscular mycorrhizal fungi (AMF) is of
   great significance in improving soil properties and plant growth. In
   this study, a pot experiment of rice (Oryza sativa L) was conducted to
   evaluate the effects of biochar derived from rice husk (HBC) and wood
   (WBC) combined with AMF (Rhizophagus irregularis) on soil nutrient
   availability and rice root traits. The results showed that the presence
   of AMF significantly increased soil pH and microbial biomass carbon
   content, and also significantly increased nutrient accumulation in
   plants (except shoot N content) in the absence of biochar. Both biochar
   significantly increased mycorrhizal colonization, with greater effects
   from HBC than WBC. The correlation analysis showed that mycorrhizal
   colonization rate had a significant positive correlation with soil
   nutrient availability (P and K) and microbial biomass carbon and
   nitrogen. Compared with the application of biochar and AMF alone, the
   combination of biochar and AMF led to significant increases in soil
   NO3--N content, available K content, and total P content, while
   significantly decreased in NH4+-N content. The positive effects of
   biochar and AMF together on plant nutrient uptake and root growth were
   more than when either biochar or AMF was present. The increase of plant
   nutrient accumulation was driven by the increase of mycorrhizal
   colonization and the improvement of root structure guided by the change
   of soil nutrient availability by biochar and AMF. Overall, this study
   indicated that the biochar combined with AMF improved the soil nutrient
   availability and root growth strategy, and then promoted the nutrient
   absorption capacity of rice.},
