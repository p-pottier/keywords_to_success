@article{ WOS:000893296600007,
Author = {Hudson, K. and Oliver, M. J. and Kohut, J. and Dinniman, M. S. and
   Klinck, J. M. and Cimino, M. A. and Bernard, K. S. and Statscewich, H.
   and Fraser, W.},
Title = {A subsurface eddy associated with a submarine canyon increases
   availability and delivery of simulated Antarctic krill to penguin
   foraging regions},
Journal = {MARINE ECOLOGY PROGRESS SERIES},
Year = {2022},
Volume = {702},
Pages = {105-122},
Month = {DEC 8},
Abstract = {The distribution of marine zooplankton depends on both ocean currents
   and swimming behavior. Many zooplankton perform diel vertical migration
   (DVM) between the surface and subsurface, which can have different
   current regimes. If concentration mechanisms, such as fronts or eddies,
   are present in the subsurface, they may impact zooplankton near-surface
   distributions when they migrate to near-surface waters. A subsurface,
   retentive eddy within Palmer Deep Canyon (PDC), a submarine canyon along
   the West Antarctic Peninsula (WAP), retains diurnal vertically migrating
   zooplankton in previous model simulations. Here, we tested the
   hypothesis that the presence of the PDC and its associated subsurface
   eddy increases the availability and delivery of simulated Antarctic
   krill to nearby penguin foraging regions with model simulations over a
   single austral summer. We found that the availability and delivery rates
   of simulated krill to penguin foraging areas adjacent to PDC were
   greater when the PDC was present compared to when PDC was absent, and
   when DVM was deepest. These results suggest that the eddy has potential
   to enhance krill availability to upper trophic level predators and
   suggests that retention may play a significant role in resource
   availability for predators in other similar systems along the WAP and in
   other systems with sustained subsurface eddies.},
