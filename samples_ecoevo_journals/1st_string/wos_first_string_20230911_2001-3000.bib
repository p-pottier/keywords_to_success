@article{ WOS:000863052100004,
Author = {Brito, Carolina and Mantuano, Dulce and Toni, Karen L. G. De and
   Mantovani, Andre},
Title = {Untangling leaf expansion triggers: A new experimental study with
   Epipremnum aureum (Araceae)},
Journal = {FLORA},
Year = {2022},
Volume = {295},
Month = {OCT},
Abstract = {Allomorphic aroid vines exhibit a distinct morphology compared to the
   typical ``sun and shade{''} leaf pattern. Their leaves exposed to high
   light irradiance at canopies are up to 30 times larger in area compared
   to leaves in the forest understory. Growth axis direction, contact with
   the host, and canopy light are morphogenesis-inducing factors for these
   aroids. While the first two factors are directly related to growth, the
   light obtained at canopies is associated with foraging and reproduction.
   In previous studies, the stimulus of light or the simultaneous
   application of contact and growth axis direction was not able to induce
   intense changes in leaf, petiole, and stem dimensions, as seen in
   nature. In this study, we tested the simultaneous interaction of light
   intensity, growth axis direction, and contact as inducing factors of
   morphological shoot development in the allomorphic aroid vine,
   Epipremnum aureum. An experiment with 60 cuttings was established, which
   included three growth conditions (horizontal, hanging, and climbing) and
   two light conditions (low light and high light). After 10 months, the
   leaf, petiole, and internode dimensions and dry mass allocation were
   compared. In both the horizontal and hanging treatments, the presence of
   high light doubled the individual leaf area to approximately 60 cm(2).
   However, when high light was combined with vertical growth and contact
   stimuli in the climbing treatment, this increase was 20-fold, producing
   leaves up to 1,000 cm(2). Dry mass allocation followed the same pattern.
   This result shows that the enormous leaf area expansion of E. aureum is
   a synergistic effect of the simultaneous application of light, growth
   axis direction, and contact. The increase in light interception by wider
   leaves is an advantage to the maintenance of allomorphic aroid vines in
   a variable light environment such as in canopies.},
