@article{ WOS:000860210000002,
Author = {Yamamoto, Masahiro and Takaki, Yoshihiro and Kashima, Hiroyuki and
   Tsuda, Miwako and Tanizaki, Akiko and Nakamura, Ryuhei and Takai, Ken},
Title = {In situ electrosynthetic bacterial growth using electricity generated by
   a deep-sea hydrothermal vent},
Journal = {ISME JOURNAL},
Year = {2023},
Volume = {17},
Number = {1},
Pages = {12-20},
Month = {JAN},
Abstract = {Electroautotrophic microorganisms have attracted great attention since
   they exhibit a new type of primary production. Here, in situ
   electrochemical cultivation was conducted using the naturally occurring
   electromotive forces at a deep-sea hydrothermal vent. The voltage and
   current generation originating from the resulting microbial activity was
   observed for 12 days of deployment, with fluctuation in response to
   tidal cycles. A novel bacterium belonging to the genus Thiomicrorhabdus
   dominated the microbial community specifically enriched on the cathode.
   Metagenomic analysis provided the draft genome of the bacterium and the
   gene repertoire indicated that the bacterium has the potential for
   thio-autotrophic growth, which is a typical physiological feature of the
   members of the genus, while the bacterium had a unique gene cluster
   encoding multi-heme cytochrome c proteins responsible for extracellular
   electron transfer. Herein, we propose this bacterium as a new species,
   specifically enriched during electricity generation, as `Candidatus
   Thiomicrorhabdus electrophagus'. This finding suggests the natural
   occurrence of electrosynthetic microbial populations using the
   geoelectricity in deep-sea hydrothermal environments.},
