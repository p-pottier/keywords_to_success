   Chatterjee, Payel; Mohan, Umesh; Sane, Sanjay P., Tata Inst Fundamental Res, Natl Ctr Biol Sci, Bangalore 560065, India.},
DOI = {10.1098/rsbl.2022.0199},
Article-Number = {20220199},
ISSN = {1744-9561},
EISSN = {1744-957X},
Keywords = {multimodal integration; gaze stabilization reflex; head positioning;
   insect vision; antennal mechanosensation; oleander hawkmoth Daphnis
   nerii},
Keywords-Plus = {BLOWFLY CALLIPHORA; MECHANISMS; VISION; MOTION; FLIGHT},
