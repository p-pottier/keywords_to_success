@article{ WOS:000890048300001,
Author = {Shi, Yangyang and Zhao, Hongyong and Zhang, Xuebing},
Title = {Dynamics of a multi-strain malaria model with diffusion in a periodic
   environment},
Journal = {JOURNAL OF BIOLOGICAL DYNAMICS},
Year = {2022},
Volume = {16},
