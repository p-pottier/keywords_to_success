@article{ WOS:000862693800002,
Author = {Clarke, Anita Elizabeth and Rimgail-Voicik, Radvile and Paskauskas,
   Ricardas and Mazeika, Jonas},
Title = {d(13)C in above-ground and below-ground organs of Spinulum annotinum
   (Lycopodiaceae)},
Journal = {FLORA},
Year = {2022},
Volume = {294},
Month = {SEP},
Abstract = {Physiological processes in plants and plant-environment interactions can
   be analysed from the composition of stable carbon isotopes (C-13/C-12).
   This ratio can be indicated using the 8 (delta) notation. Substantial
   evidence has been published in recent years demonstrating that
   post-photosynthetic fractionation occurs in plants and leads to
   differences in C-13-enrichment in heterotrophic (as compared with
   autotrophic) organs. Yet our understanding of processes leading to C-13
   divergence between leaves and heterotrophic tissues remains low. Large
   part of the isotope analysis is concerned with angiosperms, but not
   spore-bearing tracheophytes. In this study, we explored the C-12 and
   C-13 isotope distribution in above-ground and below-ground organs of an
   archaic lycophyte Spinulum annotinum (L.) A.Haines (interrupted
   clubmoss). The main objectives of this study were to determine the
   natural distribution of stable carbon isotopes in sporophytes of S.
   annotinum and to discuss possible causes for differences in delta C-13 .
   The study revealed that there are significant differences between delta
   C-13 in different S. annotinum organs. The delta C-13 values varied by
   about 2.4\%o. The highest delta C-13 values were -30.0\%o in roots and
   the lowest were-32.4\%o in plagiotropic shoot leaves. The distance to a
   body of water had no significant effect on delta C-13 delta C-13 values
   in S. annotinum. Outgrowth size did not have a clear relationship with
   delta C-13 in S. annotinum. The Shannon Diversity Index was a
   significant predictor for delta C-13 values in S. annotinum. The
   uncovered delta C-13 and the relationship with abiotic habitat
   conditions and neighbouring plant species in the community provide a
   base for future lycophyte ecophysiology research.},
