@article{ WOS:000875853200004,
Author = {Xu, Tongtong and Abdullah, Iram and Akram, Nudrat Aisha and Wang, Ying
   and Zhang, Li and Feng, Xinyue and Wang, Jianyong and Wang, Ling},
Title = {Defoliation facilitates Leymus chinensis clones spatial expansion into
   saline-alkali soils under different population densities},
Journal = {FLORA},
Year = {2022},
Volume = {296},
Month = {NOV},
Abstract = {Defoliation/grazing and competition could have impacts on growth and
   spatial expansion of clonal plants. However, how these factors
   influencing clonal plants spatial expansion in heterogeneous
   environments remain ambiguous. A pot experiment was conducted to examine
   the effects of defoliation, intraspecific competition and their
   interactions on the spatial expansion of Leymus chinensis (Trin.)
   Tzvel., a dominant clonal plant species in the Songnen grassland,
   northeast China, from favourable soil patches to saline-alkali soil
   patches. Results showed that defoliation significantly increased L.
   chinensis spatial expansion from favourable soil patches into saline
   -alkali ones. The average rhizome length increased, as did the number of
   ramets growing in saline-alkali soil patches. Intraspecific competition
   had no documented effects on these parameters. Defoliation increased the
   total biomass of the ramets in moderate and severe saline-alkali patches
   by 33\% and 22\%, respectively. Across the saline-alkali stress regimes,
   defoliation increased the belowground biomass by 26\%, while increased
   aboveground biomass by 18\%. Results demonstrated that defoliation
   improved L. chinensis expansion in saline -alkali soil patches mainly
   due to increased belowground biomass, which contributes to rhizome
   elongation and ramet proliferation. Therefore, it is suggested that
   defoliation/mowing management practices in Songnen degraded grasslands
   might promote the expansion and colonization of L. chinensis clones
   toward degraded saline -alkali patches, thereby accelerating vegetative
   reconstruction and grassland restoration.},
