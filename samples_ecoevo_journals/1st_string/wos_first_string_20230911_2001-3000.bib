@article{ WOS:000874967100002,
Author = {O'Brien, Michael J. and Carbonell, Elisa P. and Schob, Christian},
Title = {Aprisco Field Station: the spatial structure of a new experimental site
   focused on agroecology},
Journal = {JOURNAL OF PLANT ECOLOGY},
Year = {2022},
Volume = {15},
Number = {6},
Pages = {1117-1123},
Month = {DEC 1},
Abstract = {The Dehesa ecosystem provides important social and economic values
   across the Iberian Peninsula. Assessing the temporal dynamics of this
   system under climate change is important for the maintenance and
   conservation of these highly valuable ecosystems. Here, we present the
   baseline data of an observational plot network in the Dehesa that will
   form the foundation for monitoring long-term dynamics and for
   experimental manipulations testing the mechanisms driving resilience
   within the Dehesa. The initial surveys indicate that the forest
   structure is typical for the Dehesa, which suggests it is an exemplary
   site for examining temporal dynamics of this ecosystem. We present these
   initial data to encourage collaborations from international scientists
   via either direct experimental projects or meta-analyses.},
