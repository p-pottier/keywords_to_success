Author-Email = {esd63@comell.edu},
Affiliations = {Cornell University},
Funding-Acknowledgement = {Washington Chapter of the Wildlife Society; Huxley College of the
   Environment},
Funding-Text = {I would like to sincerely thank C. Johnson for assisting with data
