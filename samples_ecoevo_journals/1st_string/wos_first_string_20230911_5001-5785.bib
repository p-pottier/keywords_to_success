@article{ WOS:000885899800002,
Author = {Rossi, David and Kuusela, Olli-Pekka and Dunn, Christopher},
Title = {A microeconometric analysis of wildfire suppression decisions in the
   Western United States},
Journal = {ECOLOGICAL ECONOMICS},
Year = {2022},
Volume = {200},
Month = {OCT},
Abstract = {Full suppression strategies remain the dominant option in wildfire
   management, despite a large body of research demonstrating the
   ecological and economic benefits of allowing unplanned wildfires to burn
   under favorable conditions. Consequently, empirical research identifying
   and understanding the factors that contribute to these decisions within
   public land management agencies has become of critical importance in
   efforts to improve management outcomes. This paper assesses the
   importance of a set of institutional and socioeconomic factors with a
   random utility model of incident commander suppression decisions. We
   compare the importance of these factors on chosen strategies before and
   after an update to federal fire policy implementation guidance and an
   associated change to federal fire budgeting policy implemented in Fiscal
   Year 2010. We find that while the update to federal fire policy guidance
   may have been effective at increasing the probability that managers
   adopted a strategy other than full suppression, the subsequent increase
   in suppression budget allotments offset this impact, rendering no true
   difference in probabilities after the policy change.},
Publisher = {ELSEVIER},
Address = {RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS},
Type = {Article},
Language = {English},
Affiliation = {Rossi, D (Corresponding Author), North Carolina State Univ, Dept Forestry \& Environm Resources, Raleigh, NC 27695 USA.
