@article{ WOS:000874959600003,
Author = {Kong, Degang and Yu, Shaohua and Tian, Jinli and Zhao, Wei and Wang, Lu
   and Zhou, Honglei},
Title = {Phytochemical investigation on Raphanus sativus L},
Journal = {BIOCHEMICAL SYSTEMATICS AND ECOLOGY},
Year = {2022},
Volume = {105},
Month = {DEC},
Abstract = {The present work was undertaken to investigate chemical constituents and
   chemotaxonomic significance from the seeds of Raphanus sativus L. As a
   result, sixteen compounds including sinapoyl derivatives (1-7) and
   indoles (8-16) were yielded, and their chemical structures were
   elucidated by H-1 NMR and C-13 NMR spectroscopic methods, along with the
   comparison with relevant literature data. Of note, ethyl
   2,3-dihydro-2-oxo-1H-indole-3-acetate (compound 11) was a new natural
   product. Three compounds (12, 13, and 16) were first isolated from the
   genus Raphanus; while five compounds (5, 9, 10, 14, and 15) were first
   reported from the family Brassi-caceae. In addition, the chemotaxonomic
   significance of isolated compounds was also discussed.},
