@article{ WOS:000864677500001,
Author = {Chen, Xiang and Liu, Ling and Yan, Wenming and Li, Minjuan and Guo,
   Zhenying and Li, Qi and He, Xiangyu and Zhao, Ziyi and Liu, Ruiyan and
   Zhang, Shunting and Huang, Yanfen and Jiang, Feng},
Title = {Effect of lanthanum modified bentonite capping on control of sediment
   phosphorus and tungsten release},
Journal = {ECOLOGICAL ENGINEERING},
Year = {2022},
Volume = {185},
Month = {DEC},
Abstract = {In this study, adsorption and incubation experiments were conducted to
   assess the capacity of LMB to adsorb phosphorous (P) and tungsten (W).
   The results of the adsorption experiment indicated that the mechanisms
   of P adsorption by LMB was chemisorption mechanism and the mechanisms of
   W adsorption by LMB were the pre-cipitation and ligand exchange,
   respectively. The results of the incubation experiment showed that the
   decreasing rates of sediment SRP and soluble W were 91\% and 65\%,
   respectively. The analysis of P and W fractionation indicated that the
   LMB capping can promote the transformation of P and W into HCl-P and
   amorphous and poorly-crystalline(oxyhydr)oxides-bound W, respectively.
   Iron (Fe) absorbed by LMB plays an important role in controlling P and W
   release from sediment, in particular in W release, while manganese (Mn)
   plays a less important role than Fe in controlling internal P and W
   release from sediment treated by LMB. The results indicated that the use
   of LMB can simultaneously control P and W release from sediment.},
