@article{ WOS:000895897000001,
Author = {Schwalb, Astrid N. N. and Swearingen, David and Robertson, Josi J. J.
   and Locklin, Jason L. L. and Moore, Josiah S. S. and McGarrity, Monica},
Title = {Living on the edge: thermal limitations of zebra mussels (Dreissena
   polymorpha) in Central Texas},
Journal = {BIOLOGICAL INVASIONS},
Year = {2022},
Month = {2022 DEC 15},
Abstract = {Dreissenid mussels are successful aquatic invaders which have spread
   rapidly throughout North America following their arrival from Eurasia
   approximately 35 years ago. Reservoirs in central Texas have been more
   recently invaded by zebra mussels and these populations represent the
   southernmost edge of this species' current distribution in North
   America. The purpose of this study was to examine the distribution and
   population dynamics (recruitment and mortality) of zebra mussels in
   Canyon Lake and the effect of high temperatures and low dissolved oxygen
   on mortality of zebra mussels in three different Central Texas lakes
   (Canyon, Belton, and Stillhouse Hollow). Recruitment, both larval
   densities and juvenile settlement, varied with temperature within and
   between years. The highest settlement rate and lower mortality were
   associated with lower temperatures (e.g., year or lake with lowest
   number of days >= 30 ?), whereas hypoxia likely caused high mortality at
   greater depths in Lake Belton. Although higher summer temperatures seem
   to limit zebra mussels, their population has continued to increase and
   expand in Canyon Lake since the initial invasion, but at considerably
   lower densities than higher latitude populations This study improves our
   understanding of factors driving and limiting zebra mussel population
   dynamics in low latitude lakes, which will help to inform management
   decisions.},
