@article{ WOS:000890305700002,
Author = {Sun, Na and Yang, Huanhuan and Zhou, Miao and Hou, Yanyan and Wu, Xinru
   and Li, Wenqian and Liu, Hui and Zheng, Lijie and Sun, Jinge and Jin,
   Linxuan and Ma, Shuo and Shu, Penghua},
Title = {A review of chemical constituents from Ajuga nipponensis and their
   anti-inflammatory activities analysis},
Journal = {BIOCHEMICAL SYSTEMATICS AND ECOLOGY},
Year = {2022},
Volume = {105},
Month = {DEC},
Abstract = {Fifteen ecdysterones, thirty-six diterpenoids, four flavonoids, one
   triterpenoid, one iridoid glycoside, and four others reported of Ajuga
   nipponensis , were summarized and discussed. Three main active
   components (12 , 29 , 54) were analyzed based on network pharmacology.
   The inflammatory signaling pathways were predicted by GO annotation
   analysis, and six key inflammatory targets were obtained. Molecular
   docking results showed that compounds 12 , 29 , and 54 had strong
   interactions with target proteins AKT1, TP53, PPARG, CASP3, and PTGS2.
   In particular, compound 54 has strong hydrogen bonding and conjugation
   with amino acid residues Met348, Met364, Val339, Leu353, Cys285, Ile262,
   Ile341, Ser342, and Arg288 in the active region of PPARG, and compound
   29 has strong hydrogen bonding and alkylation with the amino acid
   residues Arg207, Leu168, Trp200, Phe256, Tyr204, Cys163, His121, and
   Met61 in the active region of CASP3.},
