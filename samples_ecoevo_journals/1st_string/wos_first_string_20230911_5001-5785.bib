@article{ WOS:000866384900001,
Author = {Serniak, L. Tucker and Chan, Samuel S. and Lajtha, Kate},
Title = {Predicting habitat suitability for Amynthas spp. in the United States: a
   retrospective analysis using citizen science data from iNaturalist},
Journal = {BIOLOGICAL INVASIONS},
Year = {2022},
Month = {2022 OCT 12},
Abstract = {Jumping worms in the Amynthas genus are of great concern as invasive
   species, but tracking new occurrences and predicting suitable habitat
   conditions have been difficult. However, citizen science projects such
   as iNaturalist can be utilized to meet these goals coupled with Species
   Distribution Modeling and publicly available habitat data. Using a
   retrospective approach, this study developed multiple predictive models
   using soil properties and land cover in addition to the standard
   WorldClim climatic variables. Occurrences observed through 2016 on
   iNaturalist were used to build several models in Maxent with different
   predictor combinations which were then assessed by their ability to
   predict occurrences submitted to iNaturalist after 2016. The model built
   using both climatic variables and soil properties greatly outperformed
   models built with climatic variables only and with climatic variables,
   soil properties, and land cover. Precipitation, isothermality,
   precipitation seasonality, and calcium carbonate soil content were all
   important predictors of suitable habitat for Amynthas worms. The
   predictive model suggests several regions in the United States and
   Canada for targeted outreach and prevention efforts. Models built using
   this method should become more accurate over time as additional
   observations are submitted to iNaturalist.},
