@article{ WOS:000877503900004,
Author = {Aqdam, Kamal Khosravi and Mahabadi, Nafiseh Yaghmaeian and Ramezanpour,
   Hassan and Rezapour, Salar and Mosleh, Zohreh and Zare, Ehsan},
Title = {Comparison of the uncertainty of soil organic carbon stocks in different
   land uses},
Journal = {JOURNAL OF ARID ENVIRONMENTS},
Year = {2022},
Volume = {205},
Month = {OCT},
Abstract = {This study was performed to compare the uncertainty spatial distribution
   of soil organic carbon (SOC) stocks in different land uses. To achieve
   this goal, 210 soil samples were collected in 1140 km(2) land areas, and
   soil carbon, pH, EC, soil texture, and mineralogy were determined. Then,
   SOC stocks and soil erodibility factor were calculated. This data set
   was combined with the environmental variables data for modeling and
   mapping of SOC stocks using the random forest model. Accordingly, the
   uncertainty https://ezproxy.ir/science/article/pii/S0016706114002006 of
   SOCof SOC stocks was calculated. The mean uncertainty of SOC stocks was
   compared in dry farming, riverbed, gardens, irrigated farming, and
   grasslands. Finally, the response surface methodology was utilized to
   analyze the effect of soil properties on the uncertainty of SOC stocks.
   The results showed that the R-2, RMSE, MAE, and concordance (Pc) for
   modeling SOC stocks by the random forest model were 0.81, 0.44 (kg.
   m(-2)), 0.34 (kg.m(-2)), and 0.83, respectively. The mean comparisons of
   uncertainty among the different land uses showed that the highest
   uncertainty was related to dry farming. The response surface methodology
   in dry farming showed that interactive effects between soil erodibility
   factor, sand, and EC were the most effective factors to address the
   uncertainty of SOC stocks.},
