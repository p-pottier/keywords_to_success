@article{ WOS:000860288000002,
Author = {Miao, Shujie and Tang, Yujie and Xue, Haiqing and Qiao, Yunfa},
Title = {Soil bacterial community responses to land-use change in Mollisol of
   Northeast China},
Journal = {ECOLOGICAL ENGINEERING},
Year = {2022},
Volume = {184},
Month = {NOV},
Abstract = {An important sensitive indicator responding to land-use change is the
   microbial community, which has been recently shown to directly influence
   soil health and function. Here, we investigated the impact of different
   land use on the soil bacterial community composition. A field experiment
   was carried out to check the changes of bacterial communities in
   response to maize cropping and fallow using Illumina MiSeq sequencing
   method. Compared to the referred natural grassland, 8-and 100-year maize
   cropping led to significant changes in the relative abundances of 5 and
   15 genera, respectively, while 8-year fallow changed the relative
   abundances of 11 genera. These changed genera resulted in distinct
   clustering of soil bacterial communities across land-use change
   according to principal coordinate analysis (PCoA). The bacterial
   community was far away from referred natural grassland with extension of
   maize cropping, and which derived by fallow was close to natural
   grassland. Based on RDA analysis, the major contributors from soil
   chemical properties to grassland samples were pH, TC, TN, MBC and
   MBN.Conclusions: These differential responses of bacterial genera are
   illustrative of the susceptibility of bacterial community to land-use
   change and given recovery possibility of bacterial community structure
   via appropriate fallow management associated with improving soil
   chemical property.},
