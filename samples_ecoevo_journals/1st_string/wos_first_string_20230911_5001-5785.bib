@article{ WOS:000874821700004,
Author = {Zhuo, Yingying and Xu, Wenxuan and Wang, Muyang and Chen, Chen and da
   Silva, Antonio Alves and Yang, Weikang and Ruckstuhl, Kathreen E. and
