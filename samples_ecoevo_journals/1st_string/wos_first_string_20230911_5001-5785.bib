@article{ WOS:000879877400002,
Author = {Hashemi, Maryamolsadat and Behboodian, Bita and Karimi, Ehsan and
   Oskoueian, Ehsan},
Title = {Azotobacter chroococcum inoculation under low drought stress condition
   improves Trachyspermum ammi seeds' essential oil bioactivity},
Journal = {BIOCHEMICAL SYSTEMATICS AND ECOLOGY},
Year = {2022},
Volume = {105},
Month = {DEC},
Abstract = {Trachyspermum ammi seed is an important spice and aromatic herb due to
   its bioactive essential oil. Drought stress and Azotobacter chroococcum
   have also been reported to improve medicinally important biomolecules
   and secondary metabolite content in plants. The present study was
   undertaken to determine the ability of A. chroococcum in combination
   with drought stress to improve the anticancer potential of T. ammi seed
   essential oil. The overall results indicated that several compounds in
   T. ammi seeds essential oil increased following A. chroococcum bacteria
   treatment under slight drought stress (irrigation at 80\% field
   capacity), thus improving the antiradical efficacy. Furthermore, the
   application of A. chroococcum as plant growth promoting agent together
   with slight drought stress significantly (p < 0.05) resulted in a
   remarkable decrease in tumor size, diminish the liver enzymes activities
   and lipid peroxidation potential in mice inoculated with TUBO cell line
   (breast cancer cell). The up-regulated caspase-3 gene expression also
   confirms the results as well. Our finding revealed that slight drought
   stress (irrigation at 80\% field capacity) together with treatment of T.
   ammi plant with A. chroococcum bacteria could be a promising approach to
   improving the pharmaceutical properties of T. ammi seeds' essential oil.},
