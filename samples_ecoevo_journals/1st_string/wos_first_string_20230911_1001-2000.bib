@article{ WOS:000860533800002,
Author = {Falkenberg, Timo and Ekesi, Sunday and Borgemeister, Christian},
Title = {Integrated Pest Management (IPM) and One Health - a call for action to
   integrate},
Journal = {CURRENT OPINION IN INSECT SCIENCE},
Year = {2022},
Volume = {53},
Month = {OCT},
Abstract = {One Health (OH) has gained considerable prominence since the beginning
   of the 21st century, among others, driven by the recent epidemics and
   the increasing importance of zoonotic diseases. Yet, despite the
   holistic and multidimensional nature of OH, to date, most emphasis has
   been on the interactions between animal and human health, with
   considerably less attention to environmental and plant health. However,
   there is growing evidence that the challenges of climate change, growing
   food and nutritional insecurity, and biodiversity loss can best be
   addressed within the context of the OH framework. Conceptionally,
   Integrated Pest Management (IPM) could perfectly fit into such an
   approach, but historically, IPM has been practiced very much in a
   compartmentalized manner. New approaches such as Regenerative
   Agriculture and Sustainable Intensification offer solutions to how to
   successfully embed IPM into a OH framework.},
