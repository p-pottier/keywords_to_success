@article{ WOS:000878692600002,
Author = {Pugacheva, A. M. and Belyaev, I, A. and Trubakova, K. Yu and Romadina,
   O. D.},
Title = {Regional Climate Changes in Arid Steppes and their Connection with
   Droughts},
Journal = {ARID ECOSYSTEMS},
Year = {2022},
Volume = {12},
Number = {4},
Pages = {353-360},
Month = {DEC},
Abstract = {Based on the long-term time series of data from weather stations located
   on chestnut soils in dry steppes of the Volgograd region, the regional
   climate changes in relation to the precipitation during active
   vegetation seasons (spring and summer) have been revealed for these
   territories. The performed regression analysis has shown an increase in
   precipitation in spring and their decrease in summer. Additionally, an
   increase in the number of droughts during the summer period with the
   simultaneous decrease in aridity has been revealed; this fact is
   associated with an increased intensity of this phenomenon described by
   the Ped index and reaching a three-unit value. An insignificant inverse
   correlation between the amount of precipitation and the number of
   drought events in the summer period in the Volgogradskiy, Surovikinskiy,
   Ilovlinskiy, and Kamyshinskiy districts or aridity events in the
   Surovikinskiy district has been observed, as well as a moderate direct
   dependence on the aridity events in the Kamyshinskiy district during a
   summer period. The areas with the maximum number of drought events and,
   therefore, with the maximum risks for agricultural production have been
   identified. These areas include the Kamyshinskiy (21 and 115 drought
   events in spring and summer, respectively) and Surovikinskiy (4 and 70
   drought events in spring and summer, respectively) districts. Taking
   into account the results we obtained for all studied territories,
   especially for those that are most affected by droughts, it is necessary
   to correct the existing crop rotation schemes and the range of
   cultivated crops as well as to shift the main emphasis towards
   reclamation farming to exclude agrarian risks during efficient use of
   local land resources.},
