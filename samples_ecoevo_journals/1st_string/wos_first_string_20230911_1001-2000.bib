@article{ WOS:000872747100001,
Author = {Rosa, Rafael Masson and Salvador, Rodrigo Brincalepe and Teixeira,
   Larissa and Bornschein, Marcos Ricardo and Cavallari, Daniel Caracanhas},
Title = {The Rapid Expansion of the Jumping Snail Ovachlamys fulgens in Brazil},
Journal = {DIVERSITY-BASEL},
Year = {2022},
Volume = {14},
Number = {10},
Month = {OCT},
Abstract = {The exotic Japanese jumping snail, Ovachlamys fulgens, was first
   recorded in Santa Catarina state, Brazil, in 2013. Based on data
   gathered from the literature, natural history collections, field
   samplings, and the iNaturalist platform, we assess its current
   distribution in the country. Our data show that the jumping snail has
   had a dramatic range expansion since its introduction, reaching six
   other states from the southern region of the country toward the Midwest.
   The affected locations are mostly urban but also include three
   biological reserves and protected areas, and possibly more. We discuss
   the consequences of this rapid invasion, its means of introduction, its
   local ecology, and prospects for monitoring and control.},
