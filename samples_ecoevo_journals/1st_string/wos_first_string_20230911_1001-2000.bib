@article{ WOS:000864173800003,
Author = {Kocaeksi, Serdar and Aksakal, Nalan and Yoney, Yigit},
Title = {Does Mental Toughness in Football Change as per the Change in League and
   Education Level?},
Journal = {AMBIENT SCIENCE},
Year = {2022},
Volume = {9},
Number = {2},
Pages = {8-10},
Abstract = {A total of 99 professional footballers belonging to two different
   Leagues participated in the study. The mean age was 24.62 +/- 4.51 and
   the mean for years of experience in professional football life was 6.21
   +/- 4.41. The Demographic Information Form and the 14-item Sports Mental
   Toughness Questionnaire-SMTQ-14 were used as study tools. The ANOVA
   analyses did not reveal any statistically significant differences as per
   the league levels for the confidence, constancy and control dimensions
   of mental toughness. But, the education levels showed a statistically
   significant difference only for the confidence dimension of mental
   toughness but not for constancy and control dimensions. It revealed,
   that there is no difference in mental toughness levels of professional
   footballers as per the leagues they play for. Interestingly, the
   confidence dimension of mental toughness levels of professional
   footballers significantly differs according to education levels.},
