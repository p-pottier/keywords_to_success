@article{ WOS:000872675200001,
Author = {Lu, Cui and Gao, Li-Zhi and Zhang, Qun-Jie},
Title = {A High-Quality Genome Assembly of the Mitochondrial Genome of the
   Oil-Tea Tree Camellia gigantocarpa (Theaceae)},
Journal = {DIVERSITY-BASEL},
Year = {2022},
Volume = {14},
Number = {10},
Month = {OCT},
Abstract = {Camellia gigantocarpa is one of the oil-tea trees whose seeds can be
   used to extract high-quality vegetable oil. To date, there are no data
   on the mitochondrial genome of the oil-tea tree, in contrast to the
   tea-tree C. sinensis, which belongs to the same genus. In this paper, we
   present the first complete mitochondrial genomes of C. gigantocarpa
   obtained using PacBio Hi-Fi (high-fidelity) and Hi-C sequencing
   technologies to anchor the 970,410 bp genome assembly into a single
   sequence. A set of 44 protein-coding genes, 22 non-coding genes, 746
   simple sequence repeats (SSRs), and more than 201 kb of repetitive
   sequences were annotated in the genome assembly. The high percentage of
   repetitive sequences in the mitochondrial genome of C. gigantocarpa
   (20.81\%) and C. sinensis (22.15\%, tea tree) compared to Arabidopsis
   thaliana (4.96\%) significantly increased the mitogenome size in the
   genus Camellia. The comparison of the mitochondrial genomes between C.
   gigantocarpa and C. sinensis revealed genes exhibit high variance in
   gene order and low substitution rate within the genus Camellia.
   Information on the mitochondrial genome provides a better understanding
   of the structure and evolution of the genome in Camellia and may
   contribute to further study of the after-ripening process of oil-tea
   trees.},
