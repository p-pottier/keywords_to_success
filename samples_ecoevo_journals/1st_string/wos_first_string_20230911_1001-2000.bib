@article{ WOS:000874229700001,
Author = {Khattak, Romaan Hayat and Teng, Liwei and Mehmood, Tahir and Ahmad,
   Shakeel and Rehman, Ejaz Ur and Basak, Sayantani M. and Liu, Zhensheng},
Title = {A Perspective of the Human-Grey Wolf (Canis lupus) Conflicts in Kumrat
