@article{ WOS:000891103900001,
Author = {Laneng, Lauretta Andrew and Tachiki, Yasuyuki and Akamatsu, Rika and
   Kobayashi, Kohei and Takahata, Chihiro and Nakamura, Futoshi},
Title = {Seasonal home range and habitat selection patterns of sika deer Cervus
   nippon in southern Hokkaido, Japan},
Journal = {WILDLIFE BIOLOGY},
Year = {2023},
Volume = {2023},
Number = {1},
Month = {JAN},
Abstract = {In 1980 and 1981, eight and nine individual sika deer Cervus nippon were
   reintroduced in southern Hokkaido, Japan, respectively, to address
   population declines in this species during 1900s. As recent population
   growth has led to human-wildlife conflicts, this study investigated the
   responses of sika deer to resource availability and geomorphic factors
   during the summer and winter seasons in southern Hokkaido. Global
   positioning system-collared data collected from 2016 to 2018 were used
   to assess the home range patterns and habitat selection of 14 female
   sika deer located in Mount Esan and Shiriuchi. The core home range size
   was defined using a 50\% kernel density estimation that indicated a
   larger home range in winter than summer for all deer. Habitat selection
   was assessed using generalized linear mixed models. The results showed
   variation in habitat selection between resident deer of Mount Esan and
   Shiriuchi, as well as migratory deer in Shiriuchi during summer.
   Resident deer in Mount Esan and Shriuchi preferred areas closer to crops
   during summer. Interaction effects revealed that migratory deer utilized
   natural grassland close to forest edge habitat in Shiriuchi. By
   contrast, resident deer in Shiriuchi selected forest edge habitat and
   natural grasslands close to crops. In winter, low elevation was the most
   important habitat factor for all deer across the study area. Thus, sika
   deer habitat selection depends on resource availability in summer and
   topographic factors in the winter.},
