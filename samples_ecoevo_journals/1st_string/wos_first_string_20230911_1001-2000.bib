@article{ WOS:000872531800001,
Author = {Paica, Ioana C. and Banciu, Cristian and Maria, Gabriel M. and
   Vladimirescu, Mihnea and Manole, Anca},
Title = {Genetic Diversity in Marginal Populations of Nitraria schoberi L. from
   Romania},
