@article{ WOS:000877022600032,
Author = {Dyatlova, A. S. and Kochenda, O. L. and Lavrov, V, N. and Korneva, E. A.},
Title = {c-Fos Expression in Rat Medulla Oblongata after Subdiaphragmatic
