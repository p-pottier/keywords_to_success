@article{ WOS:000877022600025,
Author = {Fomicheva, E. E. and Shanin, S. N. and Filatenkova, T. A. and Novikova,
   N. S. and Dyatlova, A. S. and Ishchenko, A. M. and Serebryanaya, N. B.},
Title = {Correction of Behavioral Disorders and State of Microglia with
   Recombinant IL-1 Receptor Antagonist in Experimental Traumatic Brain
   Injury},
Journal = {JOURNAL OF EVOLUTIONARY BIOCHEMISTRY AND PHYSIOLOGY},
Year = {2022},
Volume = {58},
Number = {5},
Pages = {1571-1582},
Month = {SEP},
Abstract = {Traumatic brain injury (TBI) is a multifactorial disease that can lead
   to the development of neurological diseases. To correct violations of
   physiological functions, anti-inflammatory cytokines are used, in
   particular, the IL-1 receptor antagonist (IL-1RA). The aim of this work
   is to evaluate the effectiveness of the rIL-1RA preparation for the
   correction of post-traumatic neuroinflammation. TBI in rats was
   simulated by dropping a 115 g weight from a height of 120 cm into the
   center of the parietal region, and the drug was injected subcutaneously
   at a dose of 50 mg/kg 60 min after injury. We studied blood
   corticosterone levels and behavioral responses in the ``Open field{''}
   test. To characterize the activation pattern of microglia in different
   parts of the brain, the expression of the Iba1 marker and morphological
   changes of cells were evaluated. Counting the total number and changing
   the shape and size of Iba1-positive microglial cells on 7th day after
   TBI showed that in animals treated with rIL-1RA the number of activated
   microglial cells was significantly higher than in intact animals, but
   the degree of their activation was significantly lower. Studies of CNS
   function disorders after TBI showed that motor and
   orientation-exploratory activities were significantly inhibited, which,
   together with the disturbance of the emotional status of the animals,
   indicates the development of a neurological deficit in intact rats. In
   animals treated with rIL-1RA, changes in behavioral characteristics were
   less pronounced. The decrease in neurological deficit in treated animals
   was directly related to the normalization of the state of microglia. The
   data obtained in the work indicate that the use of rIL-1RA 1 h after TBI
   allows correction of motor, orientation-exploratory activity and reduce
   microglia activation in different parts of the CNS.},
