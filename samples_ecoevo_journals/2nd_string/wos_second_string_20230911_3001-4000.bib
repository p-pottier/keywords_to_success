@article{ WOS:000807307300001,
Author = {de Figueiredo, Livia Pitombeira and Athayde, Danillo B. and da Silva
   Pinto, Thandy Junio and Daam, Michiel A. and Guerra, Glauce da Silva and
   Duarte-Neto, Paulo Jose and Espindola, Evaldo L. G.},
Title = {Influence of temperature on the toxicity of the elutriate from a
   pesticide contaminated soil to two cladoceran species},
Journal = {ECOTOXICOLOGY},
Year = {2022},
Volume = {31},
Number = {6},
Pages = {956-966},
Month = {AUG},
Abstract = {Brazil has become one of the largest consumers of pesticides in the
   world. However, there are still few studies evaluating pesticide
   toxicity integrating local aquatic and terrestrial environments. In
   addition, there is growing concern about the influence of temperature
   conditions related with climate change on contaminants toxicity. The aim
   of the present study was to evaluate the elutriate toxicity of the
   insecticide Kraft (R) 36 EC (a.i. abamectin), the fungicide Score (R)
   250 EC (a.i. difenoconazole) and their mixture to the cladocerans
   Ceriodaphnia silvestrii and Daphnia similis, using model ecosystems
   (mesocosms). To this end, mesocosms were filled with natural soil and
   subjected to the following treatments: Control (Milli-Q water), Kraft
   (10.8 g abamectin ha(-1)), Score (20 g difenoconazole ha(-1)), and Kraft
   + Score (10.8 g abamectin ha(-1) + 20 g difenoconazole ha(-1)). The
   experiment lasted 18 days, and the applications were made on days 1, 8,
   and 15; the occurrence of rainfall was simulated on days 1, 8, and 15
   after applications and only rainfall simulation on days 4, 11, and 18.
   The experiment was conducted under two different temperatures: 23
   degrees C and 33 degrees C. At 23 degrees C, single Kraft treatment and
   in combination with Score showed high toxicity to both cladocerans. At
   33 degrees C, elutriate of the Kraft (R) and mixture treatments were
   highly toxic to D. similis but not to C. silvestrii. The results
   indicate that while Kraft had higher toxicity than Score to both
   cladocerans, this toxicity was counteracted at 33 degrees C only for the
   exotic species, D. similis. The results portray the complexity of
   pesticide toxicity when considering realistic experimental settings
   including different organisms and temperature treatments.},
