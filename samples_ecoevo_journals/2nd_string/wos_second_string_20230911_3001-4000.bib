@article{ WOS:000799945400009,
Author = {Coleine, Claudia and Stajich, Jason E. and Selbmann, Laura},
Title = {Fungi are key players in extreme ecosystems},
Journal = {TRENDS IN ECOLOGY \& EVOLUTION},
Year = {2022},
Volume = {37},
Number = {6},
Pages = {517-528},
Month = {JUN},
Abstract = {Extreme environments on Earth are typically devoid of macro life forms
   and are inhabited predominantly by highly adapted and specialized
   microorganisms. The discovery and persistence of these extremophiles
   provides tools to model how life arose on Earth and inform us on the
   limits of life. Fungi, in particular, are among the most
   extreme-tolerant organisms with highly versatile lifestyles and stunning
   ecological and morphological plasticity. Here, we overview the most
   notable examples of extremophilic and stress-tolerant fungi,
   highlighting their key roles in the functionality and balance of extreme
   ecosystems. The re-markable ability of fungi to tolerate and even thrive
   in the most extreme environ-ments, which preclude most organisms, have
   reshaped current concepts regarding the limits of life on Earth.},
