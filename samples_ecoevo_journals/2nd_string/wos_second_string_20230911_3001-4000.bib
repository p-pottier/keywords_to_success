Address = {VAN GODEWIJCKSTRAAT 30, 3311 GZ DORDRECHT, NETHERLANDS},
Type = {Article},
Language = {English},
Affiliation = {Souto-Veiga, R (Corresponding Author), Univ Potsdam, Plant Ecol \& Nat Conservat, Muhlenberg 3, D-14476 Potsdam, Germany.
   Souto-Veiga, R (Corresponding Author), Murdoch Univ, Environm \& Conservat Sci, Murdoch, WA 6150, Australia.
