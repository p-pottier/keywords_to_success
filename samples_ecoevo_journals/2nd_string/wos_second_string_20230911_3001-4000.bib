@article{ WOS:000800320200001,
Author = {CORNING, P. E. T. E. R. A.},
Title = {Culture-gene co-evolution: Darwin's other theory comes into view},
Journal = {BIOLOGICAL JOURNAL OF THE LINNEAN SOCIETY},
Year = {2023},
Volume = {139},
Number = {4, SI},
Pages = {563-569},
Month = {AUG 9},
Abstract = {The concept of `natural selection', as proposed in The Origin of Species
   (published in 1859), refers to the better known of Charles Darwin's two
   distinct theories of evolution. Darwin's other theory was originally
   associated with his predecessor, Jean-Baptiste de Lamarck. (Darwin used
   the shorthand phrase, the `use and disuse of parts', to characterize
   this alternative theory.) Lamarck's theory was rejected (and even
   mocked) by many 20(th) century theorists, who instead adopted a
   gene-centred, one-way, bottom-up model of evolutionary change, called
   the modern synthesis. (It has also been termed neo-Darwinism.) Darwin
   was more receptive to Lamarck's approach, and in his second great work,
   The Descent of Man (published in 1874), he applied this Lamarckian
   theory particularly to human evolution. He ascribed our remarkable
   evolutionary success to our behaviour: social cooperation, innovation
   (and tools), morality and cumulative culture. Neo-Darwinists called it
   Darwin's biggest mistake. However, an ever-increasing body of evidence,
   inclusive of cultural phenomena in many other species, suggests that it
   was, in fact, their mistake. Often called gene-culture co-evolution
   theory, the reverse order (as used here) might be a better way of
   characterizing the underlying causal dynamics. The `purposeful'
   behaviour (teleonomy) of living systems has exerted a major influence on
   the trajectory of biological evolution.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {CORNING, PA (Corresponding Author), Inst Study Complex Syst, 900 Univ St,D-X, Seattle, WA 98101 USA.
   CORNING, P. E. T. E. R. A., Inst Study Complex Syst, 900 Univ St,D-X, Seattle, WA 98101 USA.},
DOI = {10.1093/biolinnean/blac048},
EarlyAccessDate = {MAY 2022},
ISSN = {0024-4066},
EISSN = {1095-8312},
Keywords = {cooperation; human evolution; Lamarck; modern synthesis; natural
   selection; purposeful behaviour; teleonomy; use and disuse of parts},
