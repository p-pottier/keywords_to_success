@article{ WOS:000810234900001,
Author = {Islam, Md Niraul and Ahmad, Wasim},
Title = {Description of two new and six known amphidelphic species of the genus
   Dorylaimoides Thorne and Swanger, 1936 (Nematoda: Dorylaimida:
   Tylencholaimoidea) from the Tropical Rainforest, Western Ghats, India},
Journal = {JOURNAL OF NATURAL HISTORY},
Year = {2022},
Volume = {56},
Number = {5-8},
Pages = {311-347},
Month = {FEB 16},
Abstract = {Two new and six known amphidelphic species belonging to the genus
   Dorylaimoides from the tropical rainforest, Western Ghats of India, are
   described and illustrated. Dorylaimoides brevicaudatus sp. nov. is
   characterised by having a body 0.99-1.1 mm long; lip region offset by
   slight depression; odontostyle 7.5-8.5 mu m long, ventral arm 5.0-5.5 mu
   m long, dorsal arm 6.0-6.5 mu m long; odontophore arcuate, 10.5-12.5 mu
   m long; total stylet length 18.5-20 mu m; pharynx expanding gradually
   into a cylindrical basal bulb, 25-30\% of total neck length; female
   genital system didelphic-amphidelphic; tail convex-conoid with rounded
   terminus; and males with 24.530 mu m long spicules. Dorylairnoides
   savants sp. nov. is characterised by having a body 1.3-1.4 mm long; lip
   region continuous; odontostyle 12.5-13.5 mu m long, ventral arm 8.5-9.0
   mu m long, dorsal arm 9.5-10.5 mu m long; odontophore arcuate, 14.5-16.5
   mu m long, total stylet length 27.5-29.5 mu m; pharynx expanding
   abruptly into a cylindrical basal bulb, 31-35\% of total neck length;
   female genital system didelphic-amphidelphic; tail long filiform,
   8.6-9.7 times anal body diameter long; and males with spicules 30-32 mu
   m long. Six known species of the genus Dorylaimoides - D. teres, D.
   micoletzkyi, D. parvus, D. leptura, D. malabaricus and D. mujtabai - are
   described and illustrated. Dorylaimoides teres is reported here for the
   first time from India.
