@article{ WOS:000805055700001,
Author = {Haerter, Chrystian Aparecido Grillo and Margarido, Vladimir Pavan and
   Blanco, Daniel Rodrigues and Traldi, Josiane Baccarin and Feldberg,
   Eliana and Lui, Roberto Laridondo},
Title = {Contributions to Trachelyopterus (Siluriformes: Auchenipteridae) species
   diagnosis by cytotaxonomic autapomorphies: from U2 snRNA chromosome
   polymorphism to rDNA and histone gene synteny},
Journal = {ORGANISMS DIVERSITY \& EVOLUTION},
Year = {2022},
Volume = {22},
Number = {4},
Pages = {1021-1036},
Month = {DEC},
Abstract = {Trachelyopterus is a Neotropical catfish genus with a controversial
   taxonomic history due to the morphological similarity among species.
   Cytogenetically, only classic techniques (Giemsa, C-band, and Ag-NOR),
   18S rDNA, 5S rDNA, telomeric probes, and the microsatellite (GATA)(n)
   have been applied in some species of the genus. Therefore, through an
   integrative approach with the 18S and 5S rDNAs, we describe for the
   first time in Trachelyopterus the distribution pattern of U2 snRNA and
   H3 and H4 histone genes and discuss evolutive and cytotaxonomic aspects
   of six Trachelyopterus species: T. striatulus, T. galeatus, and T.
   porosus from the Amazonas River basin, T. coriaceus and Trachelyopterus
   aff. galeatus from the Araguaia River basin, and Trachelyopterus aff.
   coriaceus from the Paraguay River basin. The 18S rDNA and U2 snRNA
   demonstrate a single locus, while 5S rDNA and histone genes had multiple
   loci. Besides that, a synteny of histone cluster with 18S rDNA locus was
   observed, and a pericentric inversion of the U2 snRNA was detected in
   Trachelyopterus aff. galeatus. Cytotaxonomically, the 5S rDNA was the
   most efficient marker and revealed chromosomal autapomorphies of four
   species, whereas the histone genes and the 18S rDNA distinguished two
   species and the U2 snRNA differentiated one species. As a result, all
   species could be distinguished, including two possible new species.},
