@article{ WOS:000857235600001,
Author = {Medeiros, Rone A. F. and Seidel, Matthias and Grossi, Paschoal C.},
Title = {Revision of the genus Byrsopolis Burmeister, 1844 (Coleoptera:
   Melolonthidae: Rutelinae: Rutelini), with the description of six new
   species endemic to Brazil and Paraguay},
Journal = {JOURNAL OF NATURAL HISTORY},
