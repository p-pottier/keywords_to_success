@article{ WOS:000824489800001,
Author = {Kull, Kalevi},
Title = {Choices by organisms: on the role of freedom in behaviour and evolution},
Journal = {BIOLOGICAL JOURNAL OF THE LINNEAN SOCIETY},
Year = {2023},
Volume = {139},
Number = {4, SI},
Pages = {555-562},
Month = {AUG 9},
Abstract = {Neo-Darwinian biology has demonstrated that it is possible to construct
   a theory of life that excludes the role of organisms' free choice. In a
   richer theory, the latter as a possibility needs to be taken into
   account. For that purpose, it is necessary to introduce the biological
   concept of choice, analyse its structure and roles, and consider some
   implications for biological theory. It is argued here that the
   conditions for free choice emerge together with umwelt-the space of
   synchronous options. Basically, choice does not require purpose. This
   leads to the conclusion that freedom is an attribute of life.},
