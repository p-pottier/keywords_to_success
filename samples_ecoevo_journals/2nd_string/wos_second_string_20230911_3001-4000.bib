@article{ WOS:000860191000001,
Author = {Oguz, Elif Kaval},
Title = {Effects of sublethal concentrations of cypermethrin on the gills of Lake
   Van Fish (Alburnus tarichi)},
Journal = {CHEMISTRY AND ECOLOGY},
Year = {2022},
Volume = {38},
Number = {10},
Pages = {900-912},
Month = {NOV 26},
Abstract = {Cypermethrin is one of the most commonly used pesticides in the Lake Van
   Basin of Turkey. In this study, the effects of a sublethal dose (0.05 mu
   g/L) of cypermethrin for 96 h on the gills of Lake Van fish (Alburnus
   tarichi), the only vertebrate species living in Lake Van, were
   investigated. Levels of heat shock protein 70 (HSP70), Na,K-ATPase, and
   DNA damage (8-OHdG) were determined immunohistochemically in the gills.
   In addition, changes in gill mucous cells and histopathological changes
   in gill tissues after 96 h were investigated. At the end of the
   cypermethrin administration, an increase was observed in the numbers of
   mitochondria-rich cells, HSP70-positive cells, and 8-OHdG-positive cells
   in the gills. A decrease in both the number and volume of mucous cells
   was detected. The expansion of blood vessels, aneurysms, lamellar
   lifting, hypertrophy, lamellar fusion, and necrosis were observed in
   gill tissues. It was concluded that the 96-h sublethal application of
   cypermethrin caused stress, deterioration of osmoregulation, DNA damage,
   and histopathological damage in the gills of Lake Van fish.},
