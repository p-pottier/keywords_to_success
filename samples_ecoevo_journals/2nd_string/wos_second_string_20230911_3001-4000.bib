Keywords = {attractors; basins; concerted action; equifinality; negative feedback;
   origin of life; persistence; plasticity; resilience; self; maintenance},
Keywords-Plus = {CHEMICAL ORGANIZATIONS; AUTOCATALYTIC SETS; COORDINATION; MECHANISM},
