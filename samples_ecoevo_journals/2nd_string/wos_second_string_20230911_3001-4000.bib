@article{ WOS:000843428800001,
Author = {Ortiz, Manuel and Winfield, Ignacio and Ardisson, Pedro-Luis},
Title = {A new deep-sea genus and species of Eriopisidae (Crustacea: Amphipoda:
   Senticaudata) from the Gulf of Mexico},
Journal = {JOURNAL OF NATURAL HISTORY},
Year = {2022},
Volume = {56},
Number = {21-24},
Pages = {1109-1121},
Month = {JUN 18},
Abstract = {A new monotypic genus of eriopisid amphipod Bathypisella gen. nov. is
   described and illustrated. The specimens were collected on sediments
   from the southern Gulf of Mexico, at 884 m depth, aboard the RN Justo
   Sierra. The new eriopisid herein described differs from other genera
   mainly by bearing coxae 1-7 with a posteroventral notch; pleonal and
   urosomal segments dorsally toothed, and without setae; pereopod 7 basis
   narrow; and telson with 2 robust setae dorsally. The new genus and
   species increase the number of eriopisid genera recorded to 19
   worldwide, and the deep-sea benthic amphipods (below 200 m) from the
   Gulf of Mexico to 20.},
