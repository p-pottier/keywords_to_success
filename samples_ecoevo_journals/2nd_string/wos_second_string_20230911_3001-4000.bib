@article{ WOS:000862892200010,
Author = {Holl, Karen D. and Luong, Justin C. and Brancalion, Pedro H. S.},
Title = {Overcoming biotic homogenization in ecological restoration},
Journal = {TRENDS IN ECOLOGY \& EVOLUTION},
Year = {2022},
Volume = {37},
Number = {9},
Pages = {777-788},
Month = {SEP},
Abstract = {Extensive evidence shows that regional (gamma) diversity is often lower
   across restored landscapes than in reference landscapes, in part due to
   common resto-ration practices that favor widespread species through
   selection of easily-grown species with high survival and propagation
   practices that reduce genetic diver-sity. We discuss approaches to
   counteract biotic homogenization, such as reintroducing species that are
   adapted to localized habitat conditions and are unlikely to colonize
   naturally; periodically reintroducing propagules from rem-nant
   populations to increase genetic diversity; and reintroducing higher
   trophic level fauna to restore interaction networks and processes that
   promote habitat heterogeneity. Several policy changes would also
   increase regional diversity; these include regional coordination amongst
   restoration groups, financial incen-tives to organizations producing
   conservation-valued species, and experimental designations for rare
   species introductions.},
