@article{ WOS:000870520700001,
Author = {Couch, Charlotte and Molmou, Denise and Magassouba, Sekou and Doumbouya,
   Saidou and Diawara, Mamadou and Diallo, Muhammad Yaya and Keita, Sekou
   Moussa and Kone, Falaye and Diallo, Mahamadou Cellou and Kourouma, Sekou
   and Diallo, Mamadou Bella and Keita, Mamady Sayba and Oulare, Aboubacar
   and Darbyshire, Iain and Gosline, George and Lughadha, Eimear Nic and
   van der Burgt, Xander and Larridon, Isabel and Cheek, Martin},
Title = {Piloting development of species conservation action plans in Guinea},
Journal = {ORYX},
Year = {2022},
Month = {2022 OCT 20},
Abstract = {Conservation action plans need to be devised and implemented if we are
   to reduce the extinction risk faced by globally threatened plants.
   However, most plant species categorized as threatened globally on the
   IUCN Red List lack conservation action plans. In West Africa, Guinea is
   one of the most diverse countries in terms of botanical species. In
   total, 273 plant species in Guinea have been assessed as being
   threatened globally, reflecting increasing pressure from the extractive
   industry and a growing population requiring food and fuel. In parallel
   with the implementation of an Important Plant Area programme in Guinea,
   we developed conservation action plans for 20 threatened plant species
   through a pilot study. We outline the methods we used and demonstrate
   the importance of adopting a collaborative approach and having
   up-to-date field information. The need for such plans is urgent, with
   recent estimates suggesting that one-third of African plants are
   threatened with extinction. Based on our experience with the first 20
   conservation action plans for Guinea species, we suggest that the
   preparation of multi-species conservation action plans would be an
   efficient use of the limited resources available for species
   conservation.},
