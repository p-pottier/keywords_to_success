@article{ WOS:000873974100010,
Author = {Vilizzi, Lorenzo and Piria, Marina and Pietraszewski, Dariusz and
   Kopecky, Oldrich and Spelic, Ivan and Radocaj, Tena and Sprem, Nikica
   and Ta, Kieu Anh T. and Tarkan, Ali Serhan and Weiperth, Andras and
