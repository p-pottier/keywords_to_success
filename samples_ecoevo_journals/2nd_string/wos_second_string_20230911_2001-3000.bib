Type = {Article},
Language = {English},
Affiliation = {Widmer, BW (Corresponding Author), Univ Missouri, Columbia, MO 65211 USA.
   Widmer, Brian W.; Gehring, Thomas M.; Heumann, Benjamin W.; Nicholson, Kirsten E., Cent Michigan Univ, Mt Pleasant, MI 48858 USA.
