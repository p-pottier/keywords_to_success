@article{ WOS:000860951200001,
Author = {Dou, Yong and Zhou, Wen L.},
Title = {Community characteristics of phytoplankton and eutrophication assessment
   in Tianjin section, downstream of Haihe River Basin},
Journal = {JOURNAL OF FRESHWATER ECOLOGY},
