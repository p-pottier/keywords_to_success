@article{ WOS:000891040700001,
Author = {Tello, Daniel and Gonzalez-Garcia, Laura Natalia and Gomez, Jorge and
   Zuluaga-Monares, Juan Camilo and Garcia, Rogelio and Angel, Ricardo and
   Mahecha, Daniel and Duarte, Erick and Leon, Maria del Rosario and Reyes,
   Fernando and Escobar-Velasquez, Camilo and Linares-Vasquez, Mario and
   Cardozo, Nicolas and Duitama, Jorge},
Title = {NGSEP 4: Efficient and accurate identification of orthogroups and
   whole-genome alignment},
Journal = {MOLECULAR ECOLOGY RESOURCES},
Year = {2023},
Volume = {23},
Number = {3},
Pages = {712-724},
Month = {APR},
Abstract = {Whole-genome alignment allows researchers to understand the genomic
   structure and variation among genomes. Approaches based on direct
   pairwise comparisons of DNA sequences require large computational
   capacities. As a consequence, pipelines combining tools for orthologous
   gene identification and synteny have been developed. In this manuscript,
   we present the latest functionalities implemented in NGSEP 4, to
   identify orthogroups and perform whole genome alignments. NGSEP
   implements functionalities for identification of clusters of homologus
   genes, synteny analysis and whole genome alignment. Our results showed
   that the NGSEP algorithm for orthogroups identification has competitive
   accuracy and efficiency in comparison to commonly used tools. The
   implementation also includes a visualization of the whole genome
   alignment based on synteny of the orthogroups that were identified, and
   a reconstruction of the pangenome based on frequencies of the
   orthogroups among the genomes. NGSEP 4 also includes a new graphical
   user interface based on the JavaFX technology. We expect that these new
   developments will be very useful for several studies in evolutionary
   biology and population genomics.},
