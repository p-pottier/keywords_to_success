@article{ WOS:000825247300007,
Author = {Graham, Sean P. and Walker, Donald and Kelehear, Crystal and Jensen,
   John B. and Dunn, Khorizon and Guyer, Craig},
Title = {Georgia Distribution and Characterization of Species Within the Eurycea
   quadridigitata Complex},
Journal = {SOUTHEASTERN NATURALIST},
