@article{ WOS:000829979200001,
Author = {Mfossa, Daniel Mbouombouo and Abwe, Ekwoge Enang and Whytock, Robin
   Cameron and Morgan, Bethan J. and Huynen, Marie-Claude and
   Beudels-Jamar, Roseline C. and Brotcorne, Fany and Tchouamo, Roger Isaac},
Title = {Distribution, habitat use and human disturbance of gorillas (Gorilla
   gorilla) in the Ebo forest, Littoral Region, Cameroon},
Journal = {AFRICAN JOURNAL OF ECOLOGY},
Year = {2022},
Volume = {60},
Number = {4},
Pages = {895-907},
Month = {DEC},
Abstract = {Great apes are threatened by many factors including habitat loss, forest
   degradation and disease. Despite the improved understanding of the
   distribution of gorillas in Cameroon, there is a lack of fine-scale
   knowledge about their habitat use. We used the Kernel Density Estimate
   method combined with grid cell counting to assess gorilla distribution,
   habitat use and human disturbance on one of the least studied gorilla
   populations in Cameroon. Proportions of different habitat types in the
   landscape were quantified using classification and regression tree
   approach applied to Landsat-8 and digital elevation model data. We found
   that the distribution of gorillas was restricted to the central band of
   the site (similar to 22 km(2)). Although the area was dominated by
   mature forest, the goodness-of-fit test showed that gorillas used
   grassland areas more frequently. Intensive human activities were noted
   across the gorilla range, with used cartridge shells being the most
   evidence hunting sign. Our results can be used to guide conservation and
   sustainable development stakeholder decisions in the area. With
   increasing anthropogenic threats throughout the landscape, we recommend
   the implementation of noninvasive research activities focusing on threat
   monitoring, nesting and dietary ecology, and the promotion of
   community-led conservation initiatives to reduce pressure on the gorilla
   habitat.},
