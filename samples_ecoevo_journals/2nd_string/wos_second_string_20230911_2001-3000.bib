@article{ WOS:000860758000001,
Author = {Milton, Suzanne J. and Short, Stefan and Dean, W. Richard J.},
Title = {Decline in whistling rat (Parotomys brantsii) density: Possible response
   to climate change in the Karoo, South Africa},
Journal = {AFRICAN JOURNAL OF ECOLOGY},
Year = {2022},
Volume = {60},
Number = {4},
Pages = {969-979},
Month = {DEC},
Abstract = {The burrow systems (warrens) of whistling rat Parotomys brantsii are
   confined to patches of deep soil (biogenic mounds known as heuweltjies)
   on stony plains in the arid Succulent Karoo region of South Africa.
   Whistling rat warrens were recorded on heuweltjies at the Tierberg
   Long-Term Ecological Research (LTER) site in the southern Karoo, Western
   Cape Province, South Africa, on eight occasions between 1988 and 2001.
   Increasing temperatures over the study period combined with a severe
   drought from 2015 to 2021 caused a 36\% dieback in perennial shrub cover
   on the research site. The percentage of heuweltjies occupied by
   whistling rats declined from 44\% occupation to 20\% occupation between
   2005 and 2021 during unusually hot, dry conditions indicated by low
   values in the Standardised Evapotranspiration Index. Around their
   warrens, whistling rats had no influence on species richness of
   perennial plants but reduced vegetation cover.},
