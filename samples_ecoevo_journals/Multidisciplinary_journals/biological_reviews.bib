@article{ WOS:001024054600001,
Author = {Reid, Rachel E. B. and Crowley, Brooke Erin and Haupt, Ryan J.},
Title = {The prospects of poop: a review of past achievements and future
   possibilities in faecal isotope analysis},
Journal = {BIOLOGICAL REVIEWS},
