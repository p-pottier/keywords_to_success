@article{ WOS:001044099200001,
Author = {Hernandez, Greco and Vazquez-Pianzola, Paula},
Title = {eIF4E as a molecular wildcard in metazoans RNA metabolism},
Journal = {BIOLOGICAL REVIEWS},
Year = {2023},
Volume = {98},
