ISSN = {0079-032X},
EISSN = {2162-4135},
Keywords = {Limulus; Euchelicerata; ventral anatomy; metastoma; homology},
Keywords-Plus = {LIMULUS-POLYPHEMUS XIPHOSURA; EVOLUTION; ONTOGENY},
