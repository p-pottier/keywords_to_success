@article{ WOS:000531484600001,
Author = {Battisti, Corrado and Amori, Giovanni and Luiselli, Luca},
Title = {Toward a new generation of effective problem solvers and
   project-oriented applied ecologists},
Journal = {WEB ECOLOGY},
Year = {2020},
Volume = {20},
Number = {1},
Pages = {11-17},
Month = {MAY 5},
Abstract = {In an era of environmental crises, conservation and management
   strategies need a new generation of applied ecologists. Here, we
   stimulate the next-generation applied ecologists to acquire a pragmatic
   mentality of problems solvers in real contexts, using the wide arsenal
   of concepts, approaches and techniques available in the project
   management (PM) arena using a road map based on the main steps of
   conservation project cycle. The acquisition of the conceptual and
   operational framework of PM can allow the next-generation applied
   ecologists to take on a more important role in nature conservation
   strategies: from data samplers, analyzers and interpreters to suppliers
   of solutions and decisions driving changes in species' targets
   inhabiting real contexts. Since the high number of applied ecologists,
   this change in approach (from analytical to operational) could make the
   difference in conservation science. We also provided, as a conceptual
   framework, a set of suggestions and approaches useful to facilitate this
   change.},
